@prefix competenciaAeroportuaria:  <http://geo.linkeddata.es/kos/transports/competenciaAeroportuaria/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix tipoDeCalzada:  <http://geo.linkeddata.es/kos/transports/tipoDeCalzada/> .
@prefix tipoDeTrafico:  <http://geo.linkeddata.es/kos/transports/tipoDeTrafico/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .

:altitud
      a       owl:DatatypeProperty ;
      rdfs:comment "Altitud en metros"@es ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Aerodromo :Aeropuerto :Helipuerto)
              ] ;
      rdfs:label "altitud"@es ;
      rdfs:range xsd:int .

<http://geo.linkeddata.es/ontology/transports.owl>
      a       owl:Ontology ;
      owl:imports <http://geo.linkeddata.es/ontology/transportes.owl> , <http://www.w3.org/2004/02/skos/core> .

anchoDeVia:internacional
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "acnho de via internacional (1.435 mm)."@es ;
      skos:inScheme transportes:anchoDeVia ;
      skos:prefLabel "ancho de vía internacional"@es .

transportes:rutaTenT
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """Esquema de conceptos que indica si el tramo de vía pertenece a la Red Transeuropea de Transporte (TEN-T)
puede tomar los valores:
* http://geo.linkeddata.es/kos/transports/rutaTenT/rutaTranseuropea
* http://geo.linkeddata.es/kos/transports/rutaTenT/rutaNoTranseuropea"""@es ;
      skos:prefLabel "ruta TEN-T"@es .

situacionDeVia:superficial
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "vía superficial"@es ;
      skos:inScheme transportes:situacionDeVia ;
      skos:prefLabel "superficial"@es .

anchoDeVia:iberico
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment " ancho ibérico (1.668 mm)"@es ;
      skos:inScheme transportes:anchoDeVia ;
      skos:prefLabel "ancho de vía ibérico"@es .

:calzada
      a       owl:ObjectProperty ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Autopista :Autovia :Carretera)
              ] ;
      rdfs:range
              [ a       owl:Class ;
                owl:intersectionOf (skos:Concept [ a       owl:Restriction ;
                            owl:hasValue transportes:tipoDeCalzada ;
                            owl:onProperty skos:inScheme
                          ])
              ] .

:CarreteraNacional
      a       owl:Class ;
      rdfs:comment "Vía de comunicación dedicada al tráfico rodado, cuya competencia corresponde a la Administración Central del Estado, clasificada como carretera de primer orden por el Ministerio de Fomento (Red de Interés General del Estado)."@es ;
      rdfs:label "Carretera Nacional"@es ;
      rdfs:subClassOf :Carretera ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:unionOf ([ a       owl:Restriction ;
                            owl:hasValue tipoDeCalzada:convencional ;
                            owl:onProperty :calzada
                          ] [ a       owl:Restriction ;
                            owl:hasValue tipoDeCalzada:sentidoUnico ;
                            owl:onProperty :calzada
                          ])
              ] .

estadoDeUso:enConstruccion
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "indica que está en construcción o en obras"@es ;
      skos:inScheme transportes:estadoDeUso ;
      skos:prefLabel "en construcción"@es .

transportes:competenciaPortuaria
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """Esquema de conceptos para definir el tipo la competencia de explotación de un puerto
puede tomar los valores:
* http://geo.linkeddata.es/kos/transports/competenciaPortuaria/estatal"""@es ;
      skos:prefLabel "competencia portuaria"@es .

acceso:libre
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "indica si la via es de acceso libre"@es ;
      skos:inScheme transportes:acceso ;
      skos:prefLabel "acceso libre"@es .

:tipoDeTrafico
      a       owl:ObjectProperty ;
      rdfs:comment """indica el tipo de tráfico que puede llevar un ferrocarril, pueden ser:
* mixto
* mercancías
* pasajeros"""@es ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:EnclaveFerroviario :FerrocarrilConvencional)
              ] ;
      rdfs:label "tipo de tráfico"@es ;
      rdfs:range
              [ a       owl:Class ;
                owl:intersectionOf (skos:Concept [ a       owl:Restriction ;
                            owl:hasValue transportes:tipoDeTrafico ;
                            owl:onProperty skos:inScheme
                          ])
              ] .

:icaoCode
      a       owl:DatatypeProperty ;
      rdfs:comment "Código de designación de aeropuertos compuesto de cuatro caracteres alfanuméricos que sirve para identificar cada aeropuerto alrededor del mundo."@es ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Aerodromo :Aeropuerto :Helipuerto)
              ] ;
      rdfs:label "icao code"@en , "código icao"@es ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:onDatatype xsd:string ;
                owl:withRestrictions
                        ([ xsd:length 4
                          ])
              ] .

[]    a       owl:AllDifferent ;
      owl:distinctMembers (rutaTenT:rutaNoTranseuropea rutaTenT:rutaTranseuropea) .

transportes:estadoDeUso
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """indica el estado de uso. Puede tomar los valores:
* http://geo.linkeddata.es/kos/transporters/estadoDeUso/enConstruccion
* http://geo.linkeddata.es/kos/transporters/estadoDeUso/enUso
* http://geo.linkeddata.es/kos/transporters/estadoDeUso/abandonado"""@es ;
      skos:prefLabel "estado de uso"@es .

electrificacion:electrificado
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "Indica que una vía está electrificada"@es ;
      skos:inScheme transportes:electrificacion ;
      skos:prefLabel "electrificado"@es .

:FerrocarrilConvencional
      a       owl:Class ;
      rdfs:comment "Anexo A: Catálogo de Objetos Geográficos de BTN100."@es , "Vía por la que circulan los trenes de RENFE que no pertenecen a la red AVE."@es ;
      rdfs:label "Tramo de vía convencional"@es ;
      rdfs:subClassOf :Ferrocarril .

:tipoDeTramo
      a       owl:ObjectProperty ;
      rdfs:comment "Tramo de vía que se desarrollan por el interior de núcleos o diseminados superficiales"@es ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Autopista :Autovia :Carretera)
              ] ;
      rdfs:label "tipo de tramo"@es ;
      rdfs:range
              [ a       owl:Class ;
                owl:intersectionOf (skos:Concept [ a       owl:Restriction ;
                            owl:hasValue transportes:tipoDeTramo ;
                            owl:onProperty skos:inScheme
                          ])
              ] .

rutaTenT:rutaTranseuropea
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "El tramo no forma parte de la Red Transeuropea de Transporte"@es ;
      skos:inScheme transportes:rutaTenT ;
      skos:prefLabel "ruta transeuropea"@es .

:unloCode
      a       owl:DatatypeProperty ;
      rdfs:comment "Código de localización de Naciones Unidas (United Nations / Location CODE – UN/LOCODE)"@es , "código unlo"@es , "UN/LOCODE, the United Nations Code for Trade and Transport Locations, is a geographic coding scheme developed and maintained by United Nations Economic Commission for Europe (UNECE), a unit of the United Nations. UN/LOCODE assigns codes to locations used in trade and transport  with functions such as seaports, rail and road terminals, airports, post offices and border crossing points."@en ;
      rdfs:domain :Puerto ;
      rdfs:label "unlo code"@en ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:onDatatype xsd:string ;
                owl:withRestrictions
                        ([ xsd:length 3
                          ])
              ] .

tipoDeTrafico:mixto
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "puede llevar mercancías o pasajeros"@es ;
      skos:inScheme transportes:tipoDeTrafico ;
      skos:prefLabel "tráfico mixto"@es .

[]    a       owl:AllDifferent ;
      owl:distinctMembers (acceso:libre acceso:peaje) .

:Helipuerto
      a       owl:Class ;
      rdfs:comment "Pista destinada al aterrizaje y despegue de helicópteros."@es ;
      rdfs:label "Helipuerto"@es ;
      terms:source "Diccionario de la Real Academia Española"@es .

:Cargadero
      a       owl:Class ;
      rdfs:comment "Instalación ferroviaria que permite el acceso de mercancías desde los trenes a los barcos. Se distingue de una estación en que no es parte de la gestión de la circulación, ni tiene que disponer de desvíos y señales."@es ;
      rdfs:label "Cargadero"@es ;
      rdfs:subClassOf :EnclaveFerroviario ;
      terms:source "Administrador de Infraestructuras Ferroviarias (ADIF)"@es .

tipoDeCalzada:sentidoUnico
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "Calzada de sentido único no contigua a la del contrario."@es ;
      skos:inScheme transportes:tipoDeCalzada ;
      skos:prefLabel "calzada de sentido único"@es .

:Telesqui
      a       owl:Class ;
      rdfs:comment " Aparato que permite a los esquiadores subir hasta las pistas sobre sus esquís mediante un sistema de arrastre."@es ;
      rdfs:label "Telesquí"@es ;
      rdfs:subClassOf :TransporteSuspendido ;
      terms:source "Diccionario de la Real Academia Española."@es .

competenciaDeVia:provincial
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "indica que la via es competencia de la Diputación Provincial"@es ;
      skos:inScheme transportes:competenciaDeVia ;
      skos:prefLabel "competencia de vía provincial"@es .

transportes:tipoDeCalzada
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """Esquema de conceptos para definir el número de calzadas y sentido de circulación del tramo de vía.
puede tomar los valores:
* http://geo.linkeddata.es/kos/transports/calzada/doble
* http://geo.linkeddata.es/kos/transports/calzada/convencional
* http://geo.linkeddata.es/kos/transports/calzada/sentidoUnico"""@es ;
      skos:prefLabel "tipo de calzada"@es .

tipoDeTramo:noInterno
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "El tramo no discurre por el interior de poblaciones o zonas de uso"@es ;
      skos:inScheme transportes:tipoDeTramo ;
      skos:prefLabel "tramo no interno"@es .

:Enlace
      a       owl:Class ;
      rdfs:comment "Tramo que une dos vías de nombre distinto y permite el acceso de una a otra."@es ;
      rdfs:label "Enlace"@es ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:unionOf ([ a       owl:Restriction ;
                            owl:hasValue estadoDeUso:enConstruccion ;
                            owl:onProperty :estadoDeUso
                          ] [ a       owl:Restriction ;
                            owl:hasValue estadoDeUso:enUso ;
                            owl:onProperty :estadoDeUso
                          ])
              ] ;
      terms:source "Anexo A: Catálogo de Objetos Geográficos de BTN100."@es .

tipoDeTramo:interno
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "El tramo no discurre por el interior de poblaciones o zonas de uso"@es ;
      skos:inScheme transportes:tipoDeTramo ;
      skos:prefLabel "tramo interno"@es .

:TransporteSuspendido
      a       owl:Class ;
      rdfs:comment "Sistema de transporte en el que los vehículos van suspendidos de un cable de tracción. Se emplea principalmente para salvar grandes diferencias de altitud."@es ;
      rdfs:label "TransporteSuspendido"@es ;
      terms:source "Anexo A: Catálogo de Objetos Geográficos de BTN100."@es .

:Autovia
      a       owl:Class ;
      rdfs:comment "Carretera con calzadas separadas para los dos sentidos de la circulación, cuyas entradas y salidas no se someten a las exigencias de seguridad de las autopistas."@es ;
      rdfs:label "Autovia"@es ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Class ;
                            owl:unionOf ([ a       owl:Restriction ;
                                        owl:hasValue tipoDeCalzada:doble ;
                                        owl:onProperty :calzada
                                      ] [ a       owl:Restriction ;
                                        owl:hasValue tipoDeCalzada:sentidoUnico ;
                                        owl:onProperty :calzada
                                      ])
                          ] [ a       owl:Class ;
                            owl:unionOf ([ a       owl:Restriction ;
                                        owl:hasValue estadoDeUso:enConstruccion ;
                                        owl:onProperty :estadoDeUso
                                      ] [ a       owl:Restriction ;
                                        owl:hasValue estadoDeUso:enUso ;
                                        owl:onProperty :estadoDeUso
                                      ])
                          ])
              ] ;
      terms:source "Anexo A: Catálogo de Objetos Geográficos de BTN100."@es .

:rutaTenT
      a       owl:ObjectProperty ;
      rdfs:comment "indica si el tramo de vía pertenece a la Red Transeuropea de Transporte (TEN-T)"@es ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Aerodromo :Aeropuerto :Autopista :Autovia :Carretera :Ferrocarril :Helipuerto)
              ] ;
      rdfs:label "ruta TEN-T"@es ;
      rdfs:range
              [ a       owl:Class ;
                owl:intersectionOf (skos:Concept [ a       owl:Restriction ;
                            owl:hasValue transportes:rutaTenT ;
                            owl:onProperty skos:inScheme
                          ])
              ] .

transportes:competenciaAeroportuaria
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """Esquema de conceptos para definir el tipo de cometencia de explotación aeroportuaria.
puede tomar los valores:
* http://geo.linkeddata.es/kos/transports/competenciaAeroportuaria/aena
* http://geo.linkeddata.es/kos/transports/competenciaAeroportuaria/militar
* http://geo.linkeddata.es/kos/transports/competenciaAeroportuaria/privada"""@es ;
      skos:prefLabel "competencia aeroportuaria"@es .

:CarreteraAutonomica
      a       owl:Class ;
      rdfs:comment "Tramo de carretera convencional de competencia autonómica o de cualquier otra administración inferior a ésta."@es ;
      rdfs:label "Carretera Autonómica"@es ;
      rdfs:subClassOf :Carretera ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Class ;
                            owl:unionOf ([ a       owl:Restriction ;
                                        owl:hasValue tipoDeCalzada:convencional ;
                                        owl:onProperty :calzada
                                      ] [ a       owl:Restriction ;
                                        owl:hasValue tipoDeCalzada:sentidoUnico ;
                                        owl:onProperty :calzada
                                      ])
                          ] [ a       owl:Class ;
                            owl:unionOf ([ a       owl:Restriction ;
                                        owl:hasValue competenciaDeVia:autonomica ;
                                        owl:onProperty :competenciaDeVia
                                      ] [ a       owl:Restriction ;
                                        owl:hasValue competenciaDeVia:local ;
                                        owl:onProperty :competenciaDeVia
                                      ] [ a       owl:Restriction ;
                                        owl:hasValue competenciaDeVia:provincial ;
                                        owl:onProperty :competenciaDeVia
                                      ])
                          ])
              ] .

[]    a       owl:AllDifferent ;
      owl:distinctMembers (situacionDeVia:elevado situacionDeVia:subterraneo situacionDeVia:superficial) .

transportes:anchoDeVia
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """Esquema de conceptos para definir el ancho de una vía
puede tomar los valores:
* http://geo.linkeddata.es/kos/transports/anchoDeVia/iberico
* http://geo.linkeddata.es/kos/transports/anchoDeVia/internacional
* http://geo.linkeddata.es/kos/transports/anchoDeVia/mixto
* http://geo.linkeddata.es/kos/transports/anchoDeVia/metrico"""@es ;
      skos:prefLabel "ancho de vía"@es .

transportes:situacionDeVia
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """Disposición del tramo de vía sobre la superficie.

* http://geo.linkeddata.es/kos/transports/situacionDeVia/elevado
* http://geo.linkeddata.es/kos/transports/situacionDeVia/subterraneo
* http://geo.linkeddata.es/kos/transports/situacionDeVia/superficial"""@es ;
      skos:prefLabel "situación"@es .

numeroDeVias:unica
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "En la vía se verifica el movimiento de los trenes en ambos sentidos"@es ;
      skos:inScheme transportes:numeroDeVias ;
      skos:prefLabel "vía única"@es .

[]    a       owl:AllDifferent ;
      owl:distinctMembers (electrificacion:electrificado electrificacion:noElectrificado) .

:situacionDeVia
      a       owl:ObjectProperty ;
      rdfs:comment "Disposición del tramo de vía sobre la superficie"@es ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Autopista :Carretera :Enlace :Ferrocarril)
              ] ;
      rdfs:label "situación"@es ;
      rdfs:range
              [ a       owl:Class ;
                owl:intersectionOf (skos:Concept [ a       owl:Restriction ;
                            owl:hasValue transportes:situacionDeVia ;
                            owl:onProperty skos:inScheme
                          ])
              ] .

transportes:tipoDeTramo
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """Esquema de conceptos que indica si el tramo de vía  se desarrolla por el interior de núcleos o diseminados superficiales
puede tomar los valores:
* http://geo.linkeddata.es/kos/transports/tipoDeTramo/interno
* http://geo.linkeddata.es/kos/transports/tipoDeTramo/noInterno"""@es ;
      skos:prefLabel "tipo de tramo"@es .

tipoDeItinerario:itinerarioNoEuropeo
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "indica que el tramo de vía no pertenece a la Red de Carreteras Europeas"@es ;
      skos:inScheme transportes:tipoDeItinerario ;
      skos:prefLabel "itinerario no europeo"@es .

tipoDeTrafico:mercancias
      a       owl:NamedIndividual , skos:Concept ;
      skos:inScheme transportes:tipoDeTrafico ;
      skos:prefLabel "tráfico de mercancías"@es .

estadoDeUso:enUso
      a       owl:NamedIndividual , skos:Concept ;
      skos:inScheme transportes:estadoDeUso ;
      skos:prefLabel "en uso"@es .

tipoDeTrafico:viajeros
      a       owl:NamedIndividual , skos:Concept ;
      skos:inScheme transportes:tipoDeTrafico ;
      skos:prefLabel "tráfico de viajeros"@es .

:Itinerario
      a       owl:Class ;
      rdfs:comment "Tramo de vía perteneciente al Camino de Santiago o catalogada como Vía verde."@es ;
      rdfs:label "Itinerario"@es ;
      terms:source "Anexo A: Catálogo de Objetos Geográficos de BTN100."@es .

transportes:categoriaVial
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """Esquema de conceptos para definir el tipo de categoría de una vía
según su importancia. Puede tomar los valores:
* http://geo.linkeddata.es/kos/transports/categoriaVial/primerOrden
* http://geo.linkeddata.es/kos/transports/categoriaVial/segundoOrden
* http://geo.linkeddata.es/kos/transports/categoriaVial/tercerOrden"""@es ;
      skos:prefLabel "categoría vial"@es .

:estadoDeUso
      a       owl:ObjectProperty ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Autopista :Autovia :Carretera :Enlace :Ferrocarril :Puerto)
              ] ;
