@prefix :        <http://geo.linkeddata.es/ontology/> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl2xml:  <http://www.w3.org/2006/12/owl2-xml#> .
@prefix hydro-ontology:  <http://geo.linkeddata.es/ontology/hydro-ontology.owl#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix geopolitical:  <http://aims.fao.org/aos/geopolitical.owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

:Canalillo
      a       owl:Class ;
      rdfs:comment "Canal pequenno de conduccion de aguas"^^xsd:string ;
      rdfs:label "Canalillo"@es ;
      rdfs:subClassOf :CanalAguasContinentales ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :conduce ;
                owl:someValuesFrom :AguasSuperficiales
              ] ;
      dc:source "Moliner, Maria. Diccionario de uso del espannol. Madrid: Editorial Gredos. 1984. p. 1590. ISBN: 84-249-1344-2"^^xsd:string .

:superficie_confinada
      a       owl:DatatypeProperty ;
      rdfs:domain :AguasSubterraneas , :AguasQuietas ;
      rdfs:range xsd:float .

:Regato
      a       owl:Class ;
      rdfs:comment "Arroyo muy pequeño"@es ;
      rdfs:label "Regato"@es ;
      rdfs:subClassOf :Arroyo ;
      dc:source "Moliner, Maria. (1984) Diccionario de uso del espannol. Madrid: Editorial Gredos. pp. 1590. ISBN: 84-249-1344-2"^^xsd:string .

:CapaDeHielo
      a       owl:Class ;
      rdfs:comment "Hielo en la superficie libre de una masa de agua, por ejemplo un lago o un rio"^^xsd:string ;
      rdfs:label "Campo de hielo"@es ;
      rdfs:subClassOf :Nevero ;
      :provenance "Glosario Hidrologico Internacional - UNESCO"^^xsd:string ;
      dc:source "Glosario Hidrologico Internacional - UNESCO"^^xsd:string .

:fondo
      a       owl:DatatypeProperty ;
      rdfs:domain :AguasSubterraneas ;
      rdfs:range xsd:string .

:Ria  a       owl:Class ;
      rdfs:comment "Parte del rio proxima a su desembocadura donde las aguas dulces se mezclan con las marinas durante la marea"^^xsd:string ;
      rdfs:label "Ría"@es , "Ria"@en ;
      rdfs:subClassOf :AguasDeTransicion ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :es_comunicada_por ;
                owl:someValuesFrom :Gola
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9."^^xsd:string .

:curvatura
      a       owl:DatatypeProperty ;
      rdfs:domain :Meandro ;
      rdfs:range xsd:string .

:AltaMar
      a       owl:Class ;
      rdfs:comment "Todas aquellas partes del mar que no forman parte del mar territorial o de las aguas interiores, que aparecen situadas fuera de las doscientas millas."^^xsd:string ;
      rdfs:label "Alta mar"@es , "Aguas Internacionales"@es ;
      rdfs:subClassOf :AguasMarinas ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :parte_de ;
                owl:someValuesFrom :Mar
              ] ;
      dc:source "Diez de Velasco, Manuel (1999) Instituciones de derecho internacional publico. Duodecima edicion. Madrid: Tecnos. 924 p. ISBN: 84-309-3446-4"^^xsd:string .

:CapaDeNieve
      a       owl:Class ;
      rdfs:comment "Nieve acumulada en el terreno."^^xsd:string ;
      rdfs:label "Campo de nieve"@es ;
      rdfs:subClassOf :Nevero ;
      :provenance "Glosario Hidrologico Internacional - UNESCO"^^xsd:string ;
      dc:source "Glosario Hidrologico Internacional - UNESCO"^^xsd:string .

:Corriente
      a       owl:Class ;
      rdfs:comment "Aguas de un rio en movimiento"@es ;
      rdfs:label "Corriente"@es ;
      rdfs:subClassOf :AguasCorrientes ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (:Lago :Rio)
                        ] ;
                owl:onProperty :parte_de
              ] ;
      :provenance "Getty Thesaurus of Geographic Names"@en ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9 & Elaboracion propia"@es .

:sometido_a_descarga_por
      a       owl:ObjectProperty ;
      rdfs:domain :Humedal ;
      rdfs:range :AguasSubterraneas ;
      rdfs:subPropertyOf :sometido .

:Regajo
      a       owl:Class ;
      rdfs:comment "Arroyo pequeño"@es ;
      rdfs:label "Regajo"@es ;
      rdfs:subClassOf :Arroyo ;
      dc:source "Diccionario de la Real Academia Espannola"^^xsd:string .

:regimen
      a       owl:DatatypeProperty ;
      rdfs:comment """tabla segmentoLago columna HidroPeriodo_Cedex & Hidroperiodo
Aplicar tb a segmentoRio columna Regimen"""^^xsd:string ;
      rdfs:domain :AguasCorrientes , :AguasQuietas ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:oneOf
                        [ a       rdf:List ;
                          rdf:first "desconocido"^^xsd:string ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "permanente no fluctuante"^^xsd:string ;
                                    rdf:rest
                                            [ a       rdf:List ;
                                              rdf:first "permantene fluctuante"^^xsd:string ;
                                              rdf:rest
                                                      [ a       rdf:List ;
                                                        rdf:first "sin catalogar"^^xsd:string ;
                                                        rdf:rest
                                                                [ a       rdf:List ;
                                                                  rdf:first "temporal erratico"^^xsd:string ;
                                                                  rdf:rest
                                                                          [ a       rdf:List ;
                                                                            rdf:first "temporal estacional"^^xsd:string ;
                                                                            rdf:rest ()
                                                                          ]
                                                                ]
                                                      ]
                                            ]
                                  ]
                        ]
              ] .

:AguasCorrientes
      a       owl:Class ;
      rdfs:comment "Aguas continentales situadas en la superficie del suelo que tienen o pueden tener un movimiento notable y continuo a traves de un cauce de origen natural o artificial."^^xsd:string ;
      rdfs:label "Aguas corrientes"@es , "Curso fluvial"@es , "Flowing waters"@en , "Curso de agua"@es , "Cauce"@es , "Flujo fluvial"@es , "Corriente de agua"@es ;
      rdfs:subClassOf :AguasSuperficiales ;
      :provenance "Corriente de agua - Glosario Hidrologico Internacional - UNESCO"^^xsd:string , "Curso de agua - Data Specification. INSPIRE"^^xsd:string , "Flujo fluvial - Glosario Hidrologico Internacional. UNESCO"^^xsd:string ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:unionOf (:AguasCorrientesArtificiales :AguasCorrientesNaturales)
              ] .

:Aguazal
      a       owl:Class ;
      rdfs:comment "Espacio al que fluye o confluye agua y tiene caracter pantanoso, generalmente con vegetacion palustre"^^xsd:string ;
      rdfs:label "Aguazal"@es ;
      rdfs:subClassOf :Cienaga ;
      dc:source "Fontanillo, E. (1986) Diccionario de Geografia. Ed. Anaya. Madrid."^^xsd:string .

:AguasDulces
      a       owl:Class ;
      rdfs:comment "Aguas que, independientemente de ser o no potable, tiene un contenido de sales tal que no llega a darle sabor"@es ;
      rdfs:label "Aguas dulces"@es ;
      dc:source "Diccionario de la Real Academia Española"@es ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (:Aguas [ a       owl:Class ;
                            owl:complementOf :AguasSaladas
                          ])
              ] .

:ZonaContigua
      a       owl:Class ;
      rdfs:comment "Zona de alta mar contigua al mar territorial que no puede extenderse mas alla de veinticuatro millas marinas contadas desde las lineas de base a, a partir de las cuales se mide la anchura del mar territorial."^^xsd:string ;
      rdfs:label "Zona contigua"@es ;
      rdfs:subClassOf :AguasMarinas ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :parte_de ;
                owl:someValuesFrom :Mar
              ] ;
      dc:source "Diez de Velasco, Manuel (1999) Instituciones de derecho internacional publico. Duodecima edicion. Madrid: Tecnos. 924 p. ISBN: 84-309-3446-4"^^xsd:string .

geopolitical:territory
      a       owl:Class .

:Canyo
      a       owl:Class ;
      rdfs:comment "Corriente de agua salada. Puede ser un cauce maritimo angosto que se interna en tierra, una comunicacion entre una albufera y el mar, o un cauce despejado entre una marisma"^^xsd:string ;
      rdfs:label "Caño"@es ;
      rdfs:subClassOf :CanalAguasMarinas ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :comunica_con ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:intersectionOf (:Albufera :Mar :Marisma)
                        ]
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :estaEn ;
                owl:someValuesFrom :Marisma
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9."^^xsd:string .

:eutrofizacion
      a       owl:DatatypeProperty ;
      rdfs:domain :Embalse ;
      rdfs:range xsd:float .

dc:creator
      a       owl:AnnotationProperty .

:Saladar
      a       owl:Class ;
      rdfs:comment "Lagunazo en que se cuaja la sal en las marismas"@es ;
      rdfs:label "Saladar"@es ;
      rdfs:subClassOf :Salina ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :esta_proxima_a ;
                owl:someValuesFrom :Marisma
              ] ;
      :provenance "Getty Thesaurus of Geographic Names"@en ;
      dc:source "Diccionario de la Real Academia Española"^^xsd:string .

:desembocaEn
      a       owl:ObjectProperty ;
      rdfs:comment "Dicho de unas aguas corrientes que vierten sus aguas al mar"@es ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:CanalMarino :Rio)
              ] ;
      rdfs:range :AguasMarinas ;
      rdfs:subPropertyOf :desagua_en .

:coordenadas
      a       owl:DatatypeProperty ;
      rdfs:domain :FenomenoHidrografico , :Morfologia , :CuencaHidrografica .

:Venaje
      a       owl:Class ;
      rdfs:comment "Conjunto de venas de agua y manantiales que dan origen a un río"@es ;
      rdfs:label "Venaje"@es ;
      rdfs:subClassOf :SurgenciaNatural ;
      dc:source "Diccionario de la Real Academia Española"@es .

:Laguna
      a       owl:Class ;
      rdfs:comment "Deposito natural de agua generalmente dulce, que hace referencia o a lagos de poca extension, o a otros que aunque de mayores dimensiones superficiales son de poca profundidad"^^xsd:string ;
      rdfs:label "Lagoon"@en , "Laguna"@es ;
      rdfs:subClassOf :AguasQuietasNaturales ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :es_originado_por ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (:Glaciar :Rio)
                        ]
              ] ;
      dc:source "Fontanillo, E. (1986) Diccionario de Geografia. Ed. Anaya. Madrid & Diccionario de la Real Academia Espannola"^^xsd:string .

:parte_de
      a       owl:ObjectProperty ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:AguasDeTransicion :Corriente :Embalse :HidronimoPuntual :Morfologia)
              ] ;
      rdfs:range
              [ a       owl:Class ;
                owl:unionOf (:AguasMarinas :Arroyo :Embalse :Lago :Laguna :Rio)
              ] ;
      owl:inverseOf :consta_de .

:estado
      a       owl:DatatypeProperty ;
      rdfs:domain :Aguas ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:oneOf
                        [ a       rdf:List ;
                          rdf:first "liquido"^^xsd:string ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "solido"^^xsd:string ;
                                    rdf:rest ()
                                  ]
                        ]
              ] .

:nivel_de_explotacion
      a       owl:DatatypeProperty ;
      rdfs:domain :AguasSubterraneas ;
      rdfs:range xsd:string .

:deposita_en
      a       owl:ObjectProperty ;
      rdfs:domain :AguasCorrientesNaturales ;
      rdfs:range :Aluvion .

:Manantial
      a       owl:Class ;
      rdfs:comment "Nacimiento de las aguas"^^xsd:string ;
      rdfs:label "Spring"@en , "Manantial"@es ;
      rdfs:subClassOf :SurgenciaNatural ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :alimenta_a ;
                owl:someValuesFrom :AguasCorrientesNaturales
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :origina ;
                owl:someValuesFrom :Chortal
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :origina ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf ([ a       owl:Class ;
                                      owl:intersectionOf (:AguasCorrientesNaturales [ a       owl:Class ;
                                                  owl:complementOf :Glaciar
                                                ])
                                    ] [ a       owl:Class ;
                                      owl:intersectionOf (:AguasQuietasNaturales [ a       owl:Class ;
                                                  owl:complementOf :Bodon
                                                ] [ a       owl:Class ;
                                                  owl:complementOf :Ibon
                                                ] [ a       owl:Class ;
                                                  owl:complementOf :Lavajo
                                                ] [ a       owl:Class ;
                                                  owl:complementOf :Lucio
                                                ] [ a       owl:Class ;
                                                  owl:complementOf :MasaDeHielo
                                                ])
                                    ])
                        ]
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9"^^xsd:string .

:almacena
      a       owl:ObjectProperty ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Aljibe :Embalse)
              ] ;
      rdfs:range
              [ a       owl:Class ;
                owl:unionOf (:Arroyo :Manantial :Rio)
              ] ;
      owl:inverseOf :es_almacenado_en .

:FuenteSalada
      a       owl:Class ;
      rdfs:label "Fuente salada"@es ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf (:AguasContinentales :AguasSaladas)
              ] .

:transmite
      a       owl:ObjectProperty ;
      rdfs:domain :Acuitardo ;
      rdfs:range :AguasSubterraneas ;
      rdfs:subPropertyOf :permite .

:EstanqueSalado
      a       owl:Class ;
      rdfs:label "Estanque salado"@es ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf (:AguasContinentales :AguasSaladas)
              ] .

:discurre_sobre
      a       owl:ObjectProperty ;
      rdfs:comment ""^^xsd:string ;
      rdfs:domain :AguasCorrientesNaturales ;
      rdfs:range :AguasCorrientesNaturales .

:Lago
      a       owl:Class ;
      rdfs:comment "Gran masa permanente de agua depositada en una depresion del terreno, aunque puede haber casos en los que el lago dispone de un rio emisario que le conecta con otro lago o con el mar."^^xsd:string ;
      rdfs:label "Lago"@es , "Lake"@en ;
      rdfs:subClassOf :AguasQuietasNaturales ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :dispone_de_emisario ;
                owl:someValuesFrom :Rio
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom
                        [ a       owl:Class ;
                          owl:intersectionOf ([ a       owl:Class ;
                                      owl:unionOf (:Glaciar :Rio)
                                    ] [ a       owl:Class ;
                                      owl:complementOf :Manantial
                                    ])
                        ] ;
                owl:onProperty :es_originado_por
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :es_conectado_con ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (:Mar :Rio)
                        ]
              ] ;
      dc:source "Fontanillo, E. (1986) Diccionario de Geografia. Ed. Anaya. Madrid & Diccionario de la Real Academia Espannola"^^xsd:string .

:sometida_a_influencia_de
      a       owl:ObjectProperty ;
      rdfs:domain :AguasDeTransicion ;
      rdfs:range
              [ a       owl:Class ;
                owl:unionOf (:AguasDulces :AguasSaladas)
              ] ;
      rdfs:subPropertyOf :sometido .

:Acequia
      a       owl:Class ;
      rdfs:comment "Zanja o canal por donde son conducidas las aguas para el riego o para otros usos."^^xsd:string ;
      rdfs:label "Irrigation ditch"@en , "Acequia"@es ;
      rdfs:subClassOf :AguasCorrientesArtificiales ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom :AguasCorrientesNaturales ;
                owl:onProperty :conduce
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. Vocabulario de terminos geograficos. Madrid: Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. 1987. 89 p. ISBN: 84-505-4929-9."^^xsd:string .

:Recinto
      a       owl:Class ;
      rdfs:comment "Espacio que ha sido cercado con el proposito de contener agua"@es , "A space that has been enclosed for some purpose"@en ;
      rdfs:label "Recinto"@es ;
      rdfs:subClassOf :AguasQuietasArtificiales ;
      dc:source "WordNet"@en .

:descarga_en
      a       owl:ObjectProperty ;
      rdfs:domain :AguasSubterraneas ;
      rdfs:range :ZonaHumeda ;
      owl:inverseOf :sufre_descarga_de .

:FuenteVauclusiana
      a       owl:Class ;
      rdfs:comment "Surgencia en regiones karsticas controlada por un sifon natural y que fluye de forma intermitente."^^xsd:string ;
      rdfs:label "Fuente vauclusiana"@es ;
      rdfs:subClassOf :FuenteNatural ;
      :provenance "Glosario Hidrologico Internacional - UNESCO"^^xsd:string ;
      dc:source "Glosario Hidrologico Internacional - UNESCO"^^xsd:string .

:consta_de
      a       owl:TransitiveProperty , owl:ObjectProperty ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:AguasMarinas :Arroyo :Embalse :Lago :Laguna :Rio)
              ] ;
      rdfs:range
              [ a       owl:Class ;
                owl:unionOf (:AguasDeTransicion :Corriente :Embalse :HidronimoPuntual :Morfologia)
              ] .

:ZonaHumeda
      a       owl:Class ;
      rdfs:comment "Terreno sometido a una descarga difusa de agua subterranea sin que se manifieste forzosamente un flujo copioso de agua liquida, aunque suele ser inundable y estar generalmente cubierto de vegetacion freatofitica tipo soto o juncal"^^xsd:string ;
      rdfs:label "Zona húmeda"@es ;
      dc:source "Gonzalez Bernaldez, Fernando. (1992) Los paisajes del agua. Terminologia popular de los humedales. Madrid: J.M. Reyero Editor. 257 p. ISBN: 84-604-3756-6."^^xsd:string ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (:Humedal [ a       owl:Class ;
                            owl:unionOf ([ a       owl:Class ;
                                        owl:intersectionOf ([ a       owl:Restriction ;
                                                    owl:allValuesFrom :AguasSuperficiales ;
                                                    owl:onProperty :es_inundada_por
                                                  ] [ a       owl:Restriction ;
                                                    owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onProperty :es_inundada_por
                                                  ])
                                      ] [ a       owl:Restriction ;
                                        owl:allValuesFrom :AguasSubterraneas ;
                                        owl:onProperty :sufre_descarga_de
                                      ])
                          ])
              ] .

:naturaleza_quimica
      a       owl:DatatypeProperty ;
      rdfs:domain :AguasSubterraneas ;
      rdfs:range xsd:string .

:Piscina
      a       owl:Class ;
      rdfs:comment "Lugar descubierto para el almacenamiento de agua para la practica de la natacion"^^xsd:string ;
      rdfs:label "Swimming pool"@en , "Piscina"@es ;
      rdfs:subClassOf :Recinto ;
      dc:source "Diccionario de la Real Academia Espannola & Elaboracion propia"^^xsd:string .

:Desembocadura
      a       owl:Class ;
      rdfs:comment "Punto en el que finaliza el trazado de un curso fluvial. La desembocadura puede ser en otro curso fluvial, en un lago, en el mar, etc."@es ;
      rdfs:label "Mouth"@en , "Desembocadura"@es ;
      rdfs:subClassOf :HidronimoPuntual ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:allValuesFrom :AguasMarinas ;
                            owl:onProperty :precedeA
                          ] [ a       owl:Restriction ;
                            owl:allValuesFrom :Rio ;
                            owl:onProperty :sigueA
                          ] [ a       owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty :precedeA
                          ] [ a       owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty :sigueA
                          ])
              ] ;
      dc:source "Diccionario de la Real Academia Española & Elaboracion propia"@es .

:AguasSuperficiales
      a       owl:Class ;
      rdfs:comment "Inland waters, except groundwater; transitional waters and coastal waters, except in respect of chemical status for which it shall also include territorial waters."^^xsd:string , "Aguas que incluyen las aguas continentales, excepto las aguas subterraneas; las aguas de transicion y las aguas costeras, y, en lo que se refiere al estado quimico, tambien las aguas territoriales"^^xsd:string ;
      rdfs:label "Masa de agua superficial"@es , "Aguas Superficiales"@es , "Surface water"@en ;
      rdfs:subClassOf :AguasContinentales ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :es_distribuida ;
                owl:someValuesFrom :Distribucion
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :es_transvasada ;
                owl:someValuesFrom :CanalAguasContinentales
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :es_distribuida ;
                owl:someValuesFrom :CanalAguasContinentales
              ] ;
      :provenance "Directiva Marco del Agua. Union Europea"^^xsd:string ;
      dc:source "Directiva Marco del Agua. Union Europea"^^xsd:string , "European Parliament and the Council of the European Union. DIRECTIVE 2000/60/EC OF THE EUROPEAN PARLIAMENT AND OF THE COUNCIL for establishing a framework for Community action in the field of water policy. Brussels, 23 October 2000. p 72."^^xsd:string ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:unionOf (:AguasCorrientes :AguasDeTransicion :AguasQuietas :Surgencia)
              ] .

:Desague
      a       owl:Class ;
      rdfs:comment "Conducto o canal por donde se da salida a las aguas hacia el mar, otro rio o un lago"^^xsd:string ;
      rdfs:label "Drainpipe"@en , "Desaguadero"@es , "Desagüe"@es ;
      rdfs:subClassOf :Tuberia ;
      dc:source "Diccionario de la Real Academia Espannola & Fontanillo, E. (1986) Diccionario de Geografia. Ed. Anaya. Madrid."^^xsd:string ;
