              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:onProperty :esta_proxima_a ;
                            owl:someValuesFrom :AguasMarinas
                          ] [ a       owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty :esta_proxima_a
                          ])
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :es_alimentada_por ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (:AguasMarinas :Manantial)
                        ]
              ] ;
