                owl:unionOf (:SurgenciaArtificial :SurgenciaNatural)
              ] .

:Glaciar
      a       owl:Class ;
      rdfs:comment "Masa de hielo acumulada en las zonas de las cordilleras por encima del limite de las nieves perpetuas y cuya parte inferior se desliza muy lentamente, como si fuese un rio de hielo"@es ;
      rdfs:label "Glaciar"@es ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :alimenta_a ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (:Arroyo :Rio)
                        ]
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty :origina ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (:Ibon :Lago :Laguna)
                        ]
              ] ;
      dc:source "Diccionario de la Real Academia Española"@es ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (:AguasCorrientesNaturales [ a       owl:Restriction ;
                            owl:onProperty :es_alimentada_por ;
                            owl:someValuesFrom :MasaDeHielo
                          ] [ a       owl:Restriction ;
                            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty :es_alimentada_por
                          ])
              ] .

:es_distribuida
      a       owl:ObjectProperty ;
      rdfs:domain :AguasSuperficiales ;
      rdfs:range
              [ a       owl:Class ;
                owl:unionOf (:CanalAguasContinentales :Distribucion)
              ] ;
      rdfs:subPropertyOf :es_conducido_por .

:aportacion
      a       owl:DatatypeProperty ;
      rdfs:domain :AguasCorrientes , :AguasQuietas ;
      rdfs:seeAlso "{\"epigenico\", \"hipogenico\", \"mixto\", \"otros\"}" .

:Barranco
      a       owl:Class ;
      rdfs:comment "Quiebra profunda que hacen en la tierra las corrientes de agua"@es ;
      rdfs:label "Barranco"@es , "Gully, Ravine"@en ;
      rdfs:subClassOf :Morfologia ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (:Arroyo :Rio)
                        ] ;
                owl:onProperty :parte_de
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9."@es .

:tasa_de_recarga
      a       owl:DatatypeProperty ;
      rdfs:domain :Acuifero ;
      rdfs:range xsd:float .

:CanalMarino
      a       owl:Class ;
      rdfs:label "Canal marino"@es , "Sea channel"@en ;
      :provenance "GeoNames"^^xsd:string ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (:CanalAguasMarinas [ a       owl:Restriction ;
                            owl:onProperty :comunica_con ;
                            owl:someValuesFrom :Mar
                          ] [ a       owl:Restriction ;
                            owl:cardinality "2"^^xsd:nonNegativeInteger ;
                            owl:onProperty :comunica_con
                          ])
              ] .

:Alberca
      a       owl:Class ;
      rdfs:comment "Deposito artificial de agua, con muros de fabrica, destinado al riego"^^xsd:string ;
      rdfs:label "Alberca"@es ;
      rdfs:subClassOf :Deposito ;
      dc:source "Seco, Manuel. Diccionario del espannol actual. Madrid: Editorial Aguilar. 1999"^^xsd:string .

:HidronimoPuntual
      a       owl:Class ;
      rdfs:label "Hydrographic point"@en , "Otros elementos hidrograficos"@es , "Hydrographical feature"@en , "Hidrónimo puntual"@es ;
      rdfs:subClassOf :FenomenoHidrografico ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom :Rio ;
                owl:onProperty :parte_de
              ] ;
      :provenance "EuroRegionalMap"^^xsd:string , "Otros elementos hidrograficos - Catalogo de entidades para el Nomenclator de la Base Topografica Nacional 1:25.000 (BTN25). Instituto Geografico Nacional"^^xsd:string , "EuroGlobalMap"^^xsd:string .

:Guadi
      a       owl:Class ;
      rdfs:comment "Arroyo intermitente en el desierto"^^xsd:string ;
      rdfs:label "Guadi"@es , "Wadi"@en , "Uadi"@es ;
      rdfs:subClassOf :Rio ;
      dc:source "Novo, P.; Chicarro, F. (1957) Diccionario de geologia y ciencias afines. Tomo 1. Ed. Labor, S.A. Barcelona"^^xsd:string .

:Cienaga
      a       owl:Class ;
      rdfs:comment "Terreno donde abundan charcos y cenagales"^^xsd:string ;
      rdfs:label "Ciénaga"@es , "Zona pantanosa"@es ;
      rdfs:subClassOf :ZonaHumeda ;
      dc:source "Elaboracion propia"^^xsd:string .

:Rivera
      a       owl:Class ;
      rdfs:comment "Arroyo, pequeño caudal de agua continua que corre por la tierra."@es ;
      rdfs:label "Rivera"@es ;
      rdfs:subClassOf :Arroyo ;
      dc:source "Diccionario de la Real Academia Española"@es .

:Meandro
      a       owl:Class ;
      rdfs:comment "Curva en el curso de un ríoo, formada por la erosion y sedimentacion que causan las aguas"@es ;
      rdfs:label "Meandro"@es ;
      rdfs:subClassOf :Morfologia ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom :Rio ;
                owl:onProperty :parte_de
              ] ;
      :provenance "Glosario Hidrologico Internacional - UNESCO"^^xsd:string ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9."^^xsd:string .

:Gola
      a       owl:Class ;
      rdfs:comment "Canal por donde entran los buques en ciertos puertos o rias."@es ;
      rdfs:label "Gola"@es ;
      dc:source "Diccionario de la Real Academia Española"@es ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (:CanalAguasMarinas [ a       owl:Restriction ;
                            owl:onProperty :comunica_con ;
                            owl:someValuesFrom :Ria
                          ])
              ] .

:tipo_margen
      a       owl:DatatypeProperty ;
      rdfs:domain :AguasCorrientesNaturales ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:oneOf
                        [ a       rdf:List ;
                          rdf:first "defendida"^^xsd:string ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "natural"^^xsd:string ;
                                    rdf:rest ()
                                  ]
                        ]
              ] .

:navegable
      a       owl:DatatypeProperty ;
      rdfs:domain :Aguas ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:oneOf
                        [ a       rdf:List ;
                          rdf:first "desconocido"^^xsd:string ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "no"^^xsd:string ;
                                    rdf:rest
                                            [ a       rdf:List ;
                                              rdf:first "si"^^xsd:string ;
                                              rdf:rest ()
                                            ]
                                  ]
                        ]
              ] .

:autoridad_competente
      a       owl:DatatypeProperty ;
      rdfs:domain :Aguas ;
      rdfs:range xsd:string .

:recarga_artificial
      a       owl:DatatypeProperty ;
      rdfs:domain :AguasSubterraneas ;
      rdfs:range xsd:boolean .

:Ojo  a       owl:Class ;
      rdfs:comment "Manantial potente que forma una pequenna charca"@es ;
      rdfs:label "Ojo"@es ;
      rdfs:subClassOf :Manantial ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:allValuesFrom :Charca ;
                            owl:onProperty :origina
                          ] [ a       owl:Restriction ;
                            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty :origina
                          ])
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9 & Elaboracion propia"@es .

:Vertiente
      a       owl:Class ;
      rdfs:comment "Pendiente por donde corre o puede correr el agua."@es ;
      rdfs:label "Vertiente"@es ;
      rdfs:subClassOf :Morfologia ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (:Arroyo :Rio)
                        ] ;
                owl:onProperty :parte_de
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9 & Diccionario de la Real Academia Española"@es .

:afloramiento
      a       owl:DatatypeProperty ;
      rdfs:domain :AguasSubterraneas ;
      rdfs:range xsd:float .

:Canyada
      a       owl:Class ;
      rdfs:comment "Corriente de agua de poco caudal que suele no ser permanente."^^xsd:string ;
      rdfs:label "Cañada"@es ;
      rdfs:subClassOf :Arroyo ;
      dc:source "Diccionario de la Real Academia de la Lengua"^^xsd:string .

:precedeA
      a       owl:ObjectProperty ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Desembocadura :Rio)
              ] ;
      rdfs:range
              [ a       owl:Class ;
                owl:unionOf (:AguasMarinas :Desembocadura :Rio)
              ] ;
      rdfs:subPropertyOf :esta_proxima_a ;
      owl:inverseOf :sigueA .
