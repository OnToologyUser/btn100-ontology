                owl:intersectionOf (ontology:Humedal [ a       owl:Class ;
                            owl:unionOf ([ a       owl:Class ;
                                        owl:intersectionOf ([ a       owl:Restriction ;
                                                    owl:allValuesFrom ontology:AguasSuperficiales ;
                                                    owl:onProperty ontology:es_inundada_por
                                                  ] [ a       owl:Restriction ;
                                                    owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onProperty ontology:es_inundada_por
                                                  ])
                                      ] [ a       owl:Restriction ;
                                        owl:allValuesFrom ontology:AguasSubterraneas ;
                                        owl:onProperty ontology:sufre_descarga_de
                                      ])
                          ])
              ] .

tipoDeComponente:repetidor
      a       owl:NamedIndividual , skos:Concept ;
      skos:inScheme supply-net:tipoDeComponente ;
      skos:prefLabel "repetidor"@es .

ontology:naturaleza_quimica
      a       owl:DatatypeProperty ;
      rdfs:domain ontology:AguasSubterraneas ;
      rdfs:range xsd:string .

ontology:Piscina
      a       owl:Class ;
      rdfs:comment "Lugar descubierto para el almacenamiento de agua para la practica de la natacion"^^xsd:string ;
      rdfs:label "Swimming pool"@en , "Piscina"@es ;
      rdfs:subClassOf ontology:Recinto ;
      dc:source "Diccionario de la Real Academia Espannola & Elaboracion propia"^^xsd:string .

ontology:NucleoPoblacionalPuntual
      a       owl:Class ;
      rdfs:comment "Pertenecerán a esta clase de entidad puntual sólo los núcleos de aquellas comunidades autónomas en las que la densidad de elementos impida su representación superficial a escala. Estas comunidades son Asturias, Galicia e Islas Canarias."@es ;
      rdfs:label "Nucleo poblacional puntual"@es ;
      rdfs:subClassOf ontology:NucleoPoblacional ;
      terms:source "Anexo A: Catálogo de Objetos Geográficos de BTN100."@es ;
      owl:disjointWith ontology:NucleoPoblacionalSuperficial .

ontology:Desembocadura
      a       owl:Class ;
      rdfs:comment "Punto en el que finaliza el trazado de un curso fluvial. La desembocadura puede ser en otro curso fluvial, en un lago, en el mar, etc."@es ;
      rdfs:label "Mouth"@en , "Desembocadura"@es ;
      rdfs:subClassOf ontology:HidronimoPuntual ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:allValuesFrom ontology:AguasMarinas ;
                            owl:onProperty ontology:precedeA
                          ] [ a       owl:Restriction ;
                            owl:allValuesFrom ontology:Rio ;
                            owl:onProperty ontology:sigueA
                          ] [ a       owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty ontology:precedeA
                          ] [ a       owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty ontology:sigueA
                          ])
              ] ;
      dc:source "Diccionario de la Real Academia Española & Elaboracion propia"@es .

competenciaPortuaria:estatal
      a       owl:NamedIndividual , skos:Concept ;
      skos:inScheme transportes:competenciaPortuaria ;
      skos:prefLabel "competencia portuaria estatal"@es .

ontology:altitudOrtometrica
      a       owl:DatatypeProperty ;
      rdfs:comment "Altitud orotométrica"@es ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (ontology:EstacionPermanenteGPS ontology:VerticeGeodesico)
              ] ;
      rdfs:label "altitudOrtometrica"@es ;
      rdfs:range xsd:float .

ontology:Autopista
      a       owl:Class ;
      rdfs:comment "Carretera con calzadas separadas para los dos sentidos de la circulación, cada una de ellas con dos o más carriles, sin cruces a nivel."@es ;
      rdfs:label "Highway"@en , "Autopista"@es ;
      rdfs:subClassOf geosparql:Feature ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Class ;
                            owl:unionOf ([ a       owl:Restriction ;
                                        owl:hasValue tipoDeCalzada:doble ;
                                        owl:onProperty ontology:calzada
                                      ] [ a       owl:Restriction ;
                                        owl:hasValue tipoDeCalzada:sentidoUnico ;
                                        owl:onProperty ontology:calzada
                                      ])
                          ] [ a       owl:Class ;
                            owl:unionOf ([ a       owl:Restriction ;
                                        owl:hasValue estadoDeUso2:enConstruccion ;
                                        owl:onProperty ontology:estadoDeUso
                                      ] [ a       owl:Restriction ;
                                        owl:hasValue estadoDeUso2:enUso ;
                                        owl:onProperty ontology:estadoDeUso
                                      ])
                          ])
              ] ;
      terms:source "Anexo A: Catálogo de Objetos Geográficos de BTN100."@es .

ontology:Embalse
      a       owl:Class ;
      rdfs:comment "Gran deposito que se forma artificialmente, por lo comun cerrando la boca de un valle mediante un dique o presa, y en el que se almacenan las aguas de un rio o arroyo, a fin de utilizarlas en el riego de terrenos, en el abastecimiento de poblaciones, en la produccion de energia electrica, etc."^^xsd:string ;
      rdfs:label "Pantano"@es , "Reservoir"@en , "Embalse"@es ;
      rdfs:subClassOf ontology:Deposito ;
      dc:source "Diccionario de la Real Academia Española"@es ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:allValuesFrom
                                    [ a       owl:Class ;
                                      owl:unionOf (ontology:Arroyo ontology:Rio)
                                    ] ;
                            owl:onProperty ontology:almacena
                          ] [ a       owl:Restriction ;
                            owl:allValuesFrom ontology:Rio ;
                            owl:onProperty ontology:parte_de
                          ] [ a       owl:Restriction ;
                            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty ontology:almacena
                          ])
              ] .

ontology:CentralEolica
      a       owl:Class ;
      rdfs:comment "Instalacion industrial que constituye una agrupación de aerogeneradores que transforman la energía eólica en energía eléctrica."@es ;
      rdfs:label "Central eólica"@es ;
      rdfs:subClassOf ontology:CentralElectrica .

ontology:AguasContinentales
      a       owl:Class ;
      rdfs:comment "All standing or flowing water on the surface of the land, and all groundwater on the landward side of the baseline from which the breadth of territorial waters is measured."^^xsd:string , "Todas las aguas quietas o corrientes en la superficie del suelo y todas las aguas subterraneas situadas hacia tierra desde la linea que sirve de base para medir la anchura de las aguas territoriales."^^xsd:string ;
      rdfs:label "Inland water"@en , "Masa de agua continental"@es , "Aguas continentales"@es ;
      rdfs:subClassOf ontology:Aguas ;
      ontology:provenance "Masa de agua continental - Nomenclator Conciso. Instituto Geografico Nacional"^^xsd:string ;
      dc:source "European Parliament and the Council of the European Union. DIRECTIVE 2000/60/EC OF THE EUROPEAN PARLIAMENT AND OF THE COUNCIL for establishing a framework for Community action in the field of water policy. Brussels, 23 October 2000. p 72."^^xsd:string ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:unionOf (ontology:AguasSubterraneas ontology:AguasSuperficiales)
              ] .

capital:comunidadAutonomaNoDeProvincia
      a       owl:NamedIndividual , skos:Concept ;
      skos:inScheme capital:tipoDeCapital ;
      skos:prefLabel "capital de comunidad autónoma  y no de provincia"@es .

[]    a       owl:AllDifferent ;
      owl:distinctMembers (electrificacion:electrificado electrificacion:noElectrificado) .

ontology:cuenca
      a       owl:DatatypeProperty ;
      rdfs:domain ontology:AguasCorrientesNaturales .

ontology:Calle
      a       owl:Class ;
      rdfs:comment "Vía que se encuentra localizada dentro de un núcleo de población o Diseminado superficial, y que ayuda a identificar la estructura de manzanas de dicho núcleo/diseminado."@es ;
      rdfs:label "Calle"@es ;
      rdfs:subClassOf geosparql:Feature ;
      terms:source "Anexo A: Catálogo de Objetos Geográficos de BTN100."@es .

ontology:Captacion
      a       owl:Class ;
      rdfs:comment "Acción y efecto de apoderarse un rio de las aguas de otro inmediato, cuando, por hallarse mas bajo o tener mayor fuerza excavadora el primero, uno de sus afluentes alcanza por su cabeza a otro del rio capturado y sirve de comunicacion o emisario entre los dos. Se conoce el fenomeno de captura por el codo brusco que presenta la unión del río capturador con el capturado"@es ;
      rdfs:label "Captación"@es ;
      rdfs:subClassOf ontology:HidronimoPuntual ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:onProperty ontology:captura ;
                            owl:someValuesFrom ontology:Rio
                          ] [ a       owl:Restriction ;
                            owl:cardinality "2"^^xsd:nonNegativeInteger ;
                            owl:onProperty ontology:captura
                          ])
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:actua_de_emisario_de ;
                owl:someValuesFrom ontology:Rio
              ] ;
      dc:source "Novo, P.; Chicarro, F. (1957) Diccionario de Geologia y Ciencias afines. Tomo 1. Ed. Labor, S.A. Barcelona"@es .

ontology:Oceano
      a       owl:Class ;
      rdfs:comment "Grande y dilatado mar que cubre la mayor parte de la superficie terrestre"^^xsd:string ;
      rdfs:label "Ocean"@en , "Océano"@es ;
      rdfs:subClassOf ontology:Mar ;
      ontology:provenance "Oceanographic geographical features - Data Specification of INSPIRE"^^xsd:string ;
      dc:source "Diccionario de la Real Academia Espannola"^^xsd:string .

ontology:PatrimonioDelaHumanidad
      a       owl:Class ;
      rdfs:comment """Título conferido por la Unesco a sitios específicos del planeta (sean bosque, montaña, lago, cueva, desierto, edificación, complejo arquitectónico o
ciudad) que han sido nominados y confirmados para su inclusión en la lista mantenida por el Programa Patrimonio de la Humanidad."""@es ;
      rdfs:label "Patrimonio de la humanidad"@es ;
      rdfs:subClassOf ontology:LugarDeInteres .

ontology:CentralSolar
      a       owl:Class ;
      rdfs:comment "Instalación industrial en la que, a partir del calentamiento de un fluido mediante radiación solar y su uso en un ciclo termodinámico convencional, se produce la potencia necesaria para mover un alternador para generación de energía eléctrica "@es ;
      rdfs:label "Central solar"@es ;
      rdfs:subClassOf ontology:CentralElectrica .

ontology:AguasCorrientesNaturales
      a       owl:Class ;
      rdfs:comment "Aguas continentales situadas en la superficie del suelo que tienen o pueden tener un movimiento notable y continuo a traves de un cauce de origen natural"^^xsd:string ;
      rdfs:label "Aguas corrientes naturales"@es , "Corriente fluvial"@es , "Cursos de agua naturales"@es , "Aguas naturales"@es , "Curso natural de agua"@es , "Corriente de agua"@es , "Natural flowing water"@en ;
      rdfs:subClassOf ontology:AguasCorrientes ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:es_originado_por ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (ontology:Glaciar ontology:Manantial ontology:MasaDeHielo ontology:Nacimiento)
                        ]
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:deposita_en ;
                owl:someValuesFrom ontology:Aluvion
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (ontology:AguasCorrientes ontology:AguasQuietas)
                        ] ;
                owl:onProperty ontology:desagua_en
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (ontology:AguasCorrientes ontology:AguasQuietasNaturales ontology:Embalse ontology:Manantial)
                        ] ;
                owl:onProperty ontology:es_alimentada_por
              ] ;
      ontology:provenance "Aguas naturales - Catalogo de entidades para el Nomenclator de Base Topografica Nacional 1:25000 (BTN25). Instituto Geografico Nacional"^^xsd:string , "Corriente de agua - Glosario Hidrologico Internacional - UNESCO"^^xsd:string ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:unionOf (ontology:Arroyo ontology:Glaciar ontology:Rambla ontology:Rio)
              ] .

anchoDeVia:internacional
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "acnho de via internacional (1.435 mm)."@es ;
      skos:inScheme transportes:anchoDeVia ;
      skos:prefLabel "ancho de vía internacional"@es .

ontology:FuenteArtificial
      a       owl:Class ;
      rdfs:comment "Obra de fabrica en la que se hace brotar el agua por conduccion"^^xsd:string ;
      rdfs:label "Fuente"@es , "Fuente artificial"@es ;
      rdfs:subClassOf ontology:SurgenciaArtificial ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:allValuesFrom ontology:Tuberia ;
                            owl:onProperty ontology:es_alimentada_por
                          ] [ a       owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty ontology:es_alimentada_por
                          ])
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9"^^xsd:string .

situacionDeVia:superficial
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "vía superficial"@es ;
      skos:inScheme transportes:situacionDeVia ;
      skos:prefLabel "superficial"@es .

hydro:regimenDeLaguna
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """puede tomar los valores:
* http://geo.linkeddata.es/kos/hydrography/regimenDeLaguna/permanete
* http://geo.linkeddata.es/kos/hydrography/regimenDeLaguna/noPermanente"""@es ;
      skos:prefLabel "régimen de laguna"@es .

ontology:ZonaPortuaria
      a       owl:Class ;
      rdfs:comment "Espacio destinado y orientado especialmente al flujo de mercancías, personas, información o a dar abrigo y seguridad a aquellas embarcaciones o naves encargadas de llevar a cabo dichas tareas."@es ;
      rdfs:label "Zona portuaria"@es ;
      rdfs:subClassOf ontology:ZonaDeUsoCaracteristico .

ontology:se_extrae_para
      a       owl:ObjectProperty ;
      rdfs:domain ontology:Acuifero ;
      rdfs:range
              [ a       owl:Class ;
                owl:unionOf (ontology:FuenteArtesana ontology:FuenteArtificial ontology:Pozo)
              ] ;
      rdfs:subPropertyOf ontology:alimenta_a ;
      owl:inverseOf ontology:se_extrae .

acceso:libre
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "indica si la via es de acceso libre"@es ;
      skos:inScheme transportes:acceso ;
      skos:prefLabel "acceso libre"@es .

