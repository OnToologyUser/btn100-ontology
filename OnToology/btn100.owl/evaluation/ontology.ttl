      rdfs:subClassOf ontology:AguasQuietas ;
      ontology:provenance "Masa de agua embalsada - Nomenclator Conciso. Instituto Geografico Nacional"^^xsd:string , "Almacenamiento de agua - Catalogo de fenomenos de la Base Topografica Nacional (BTN). Instituto Geografico Nacional"^^xsd:string , "Aguas artificiales - Catalogo de entidades para el Nomenclator de la Base Topografica Nacional 1:25000 (BTN25). Instituto Geografico Nacional"^^xsd:string ;
