      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:estaEn ;
                owl:someValuesFrom ontology:Barranco
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:puede_fluir_como ;
                owl:someValuesFrom ontology:CorrienteSubterranea
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:actua_de_emisario_de ;
                owl:someValuesFrom ontology:Lago
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:es_alimentada_por ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (ontology:Glaciar ontology:Manantial ontology:MasaDeHielo)
                        ]
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (ontology:Marisma ontology:ZonaEncharcable ontology:ZonaHumeda)
                        ] ;
                owl:onProperty ontology:inunda
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom ontology:AguasCorrientesNaturales ;
                owl:onProperty ontology:tiene_afluente
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:conecta_con ;
                owl:someValuesFrom ontology:Lago
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom ontology:Captacion ;
                owl:onProperty ontology:es_capturado
              ] ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:allValuesFrom ontology:Nacimiento ;
                            owl:onProperty ontology:es_originado_por
                          ] [ a       owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty ontology:es_originado_por
                          ])
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:es_almacenado_en ;
                owl:someValuesFrom ontology:Embalse
              ] ;
