ontology:Aluvion
      a       owl:Class ;
      rdfs:comment "Suelo formado por acumulacion de detritos, tales como lodos, gravas, guijarros y arenas, depositados por las aguas en desembocaduras y cauces de los rios y arroyos"@es ;
      rdfs:label "Aluvión"@es ;
      rdfs:subClassOf ontology:Morfologia ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (ontology:Arroyo ontology:Rio)
                        ] ;
                owl:onProperty ontology:parte_de
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty ontology:estaEn
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:estaEn ;
                owl:someValuesFrom ontology:Desembocadura
              ] ;
