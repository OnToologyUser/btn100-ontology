      skos:inScheme hydro:regimenDeLaguna ;
      skos:prefLabel "permanente"@es .

ontology:Desague
      a       owl:Class ;
      rdfs:comment "Conducto o canal por donde se da salida a las aguas hacia el mar, otro rio o un lago"^^xsd:string ;
      rdfs:label "Drainpipe"@en , "Desaguadero"@es , "Desagüe"@es ;
      rdfs:subClassOf ontology:Tuberia ;
      dc:source "Diccionario de la Real Academia Espannola & Fontanillo, E. (1986) Diccionario de Geografia. Ed. Anaya. Madrid."^^xsd:string ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:allValuesFrom
                                    [ a       owl:Class ;
                                      owl:unionOf (ontology:AguasMarinas ontology:AguasQuietas ontology:Arroyo ontology:Rio)
                                    ] ;
                            owl:onProperty ontology:vierte_en
                          ] [ a       owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
