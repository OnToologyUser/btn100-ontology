              [ a       owl:Restriction ;
                owl:onProperty ontology:evacua_de ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (ontology:Acuifero ontology:ZonaEncharcable)
                        ]
              ] ;
      ontology:provenance "Glosario Hidrologico Internacional - UNESCO"^^xsd:string ;
      dc:source "Glosario Hidrologico Internacional - UNESCO"^^xsd:string ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (ontology:Conducto [ a       owl:Restriction ;
                            owl:onProperty ontology:evacua_de ;
                            owl:someValuesFrom
                                    [ a       owl:Class ;
                                      owl:unionOf (ontology:Acuifero ontology:ZonaEncharcable)
                                    ]
                          ])
              ] .

ontology:Estuario
      a       owl:Class ;
      rdfs:comment "Ancha boca de rio, donde la depresion del cauce permite la entrada de las aguas marinas en la marea, por ello, se produce mezcla de agua dulce y salada"^^xsd:string , "The wide part of a river where it nears the sea; fresh and salt water mix"^^xsd:string ;
