""" ;
      rdfs:label "geo" ;
      dc:date "$Date: 2009/04/20 15:00:30 $" ;
      dc:description """A vocabulary for representing latitude, longitude and
 altitude information in the WGS84 geodetic reference datum.
 Version $Id: wgs84_pos.rdf,v 1.22 2009/04/20 15:00:30 timbl Exp $. See http://www.w3.org/2003/01/geo/ for more details.""" ;
      dc:title "WGS84 Geo Positioning: an RDF vocabulary" .

ontology:tipologia_de_surgencia
      a       owl:DatatypeProperty ;
      rdfs:domain ontology:Surgencia ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:oneOf
                        [ a       rdf:List ;
                          rdf:first "difusa"^^xsd:string ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "puntual"^^xsd:string ;
                                    rdf:rest ()
                                  ]
                        ]
              ] .

ontology:riesgo_quimico
      a       owl:DatatypeProperty ;
      rdfs:comment "tabla MasaSubterranea_Riesgo Quimico columna sustancia"^^xsd:string ;
      rdfs:domain ontology:AguasSubterraneas ;
      rdfs:range xsd:string .

transportes:estadoDeUso
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """indica el estado de uso. Puede tomar los valores:
* http://geo.linkeddata.es/kos/transporters/estadoDeUso/enConstruccion
* http://geo.linkeddata.es/kos/transporters/estadoDeUso/enUso
* http://geo.linkeddata.es/kos/transporters/estadoDeUso/abandonado"""@es ;
      skos:prefLabel "estado de uso"@es .

ontology:Riachuelo
      a       owl:Class ;
      rdfs:comment "A natural stream of water smaller than a river (and often a tributary of a river)"@en , "Río pequeño y de poco caudal que con frecuencia es afluente de un río"@es ;
      rdfs:label "Riachuelo"@es ;
      rdfs:subClassOf ontology:Arroyo ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:es_afluente_de ;
                owl:someValuesFrom ontology:Rio
              ] ;
      dc:source "Novo, P.; Chicarro, F. (1957) Diccionario de Geologia y Ciencias afines. Tomo 1. Ed. Labor, S.A. Barcelona & Diccionario de la Real Academia Española"@es , "WordNet"@en .

ontology:grado_de_intervencion
      a       owl:DatatypeProperty ;
      rdfs:domain ontology:AguasQuietas ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:oneOf
                        [ a       rdf:List ;
                          rdf:first "alto, esencial en la funcionalidad actual"^^xsd:string ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "bajo, no afecta a la funcionalidad"^^xsd:string ;
                                    rdf:rest
                                            [ a       rdf:List ;
                                              rdf:first "desconocido"^^xsd:string ;
                                              rdf:rest
                                                      [ a       rdf:List ;
                                                        rdf:first "medio, afecta algo"^^xsd:string ;
                                                        rdf:rest
                                                                [ a       rdf:List ;
                                                                  rdf:first "sin catalogar"^^xsd:string ;
                                                                  rdf:rest ()
                                                                ]
                                                      ]
                                            ]
                                  ]
                        ]
              ] .

ontology:evacua_de
      a       owl:ObjectProperty ;
      rdfs:domain ontology:Dren ;
      rdfs:range
              [ a       owl:Class ;
                owl:unionOf (ontology:Acuifero ontology:ZonaEncharcable)
              ] .

tipoDeTrafico:mixto
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "puede llevar mercancías o pasajeros"@es ;
      skos:inScheme transportes:tipoDeTrafico ;
      skos:prefLabel "tráfico mixto"@es .

kos:btn100
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment "Tesauro SKOS creado a partir de la documentación del Catálogo de Objetos Geográficos de BTN100"@es ;
      skos:prefLabel "btn100"@es .

tipoDeCalzada:sentidoUnico
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "Calzada de sentido único no contigua a la del contrario."@es ;
      skos:inScheme transportes:tipoDeCalzada ;
      skos:prefLabel "calzada de sentido único"@es .

ontology:LagoSubterraneo
      a       owl:Class ;
      rdfs:label "Lago subterráneo"@es , "Subterranean lake"@en ;
      rdfs:subClassOf ontology:AguasSubterraneas ;
      ontology:provenance "GeoNames"^^xsd:string .

wgs84_pos:long
      a       owl:AnnotationProperty , owl:DatatypeProperty ;
      rdfs:comment "The WGS84 longitude of a SpatialThing (decimal degrees)." ;
      rdfs:domain wgs84_pos:SpatialThing ;
      rdfs:label "longitude" .

[]    a       owl:AllDifferent ;
      owl:distinctMembers (estadoDeUso2:abandonado estadoDeUso2:enConstruccion estadoDeUso2:enUso) .

ontology:provenance
      a       owl:AnnotationProperty .

ontology:LagunaSalada
      a       owl:Class ;
      rdfs:comment "Deposito natural de agua salobre y con dimensiones reducidas"@es ;
      rdfs:label "Laguna salada"@es ;
      rdfs:subClassOf ontology:Laguna , ontology:AguasSaladas ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:onProperty ontology:es_alimentada_por ;
                            owl:someValuesFrom ontology:AguasMarinas
                          ] [ a       owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty ontology:es_alimentada_por
                          ])
              ] ;
      dc:source "Diccionario de la Real Academia Española & Elaboracion propia"@es .

ontology:Lucio
      a       owl:Class ;
      rdfs:comment "Laguna formada en el interior de una marisma"@es ;
