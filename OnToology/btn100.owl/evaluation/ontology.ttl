@prefix tipoDeTramo:  <http://geo.linkeddata.es/kos/transports/tipoDeTramo/> .
@prefix geodesy:  <http://geo.linkeddata.es/kos/geodesy/> .
@prefix capital:  <http://geo.linkeddata.es/kos/populatuon-buildings/capital/> .
@prefix hydro:   <http://geo.linkeddata.es/kos/hydrography/> .
@prefix regimenDeLaguna:  <http://geo.linkeddata.es/kos/hydrography/regimenDeLaguna/> .
@prefix anchoDeVia:  <http://geo.linkeddata.es/kos/transports/anchoDeVia/> .
@prefix kos:     <http://geo.linkeddata.es/kos/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix situacion:  <http://geo.linkeddata.es/kos/supply-net/situacion/> .
@prefix geopolitical:  <http://aims.fao.org/aos/geopolitical.owl#> .
@prefix supply-net:  <http://geo.linkeddata.es/kos/supply-net/> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix btn100:  <http://geo.linkeddata.es/kos/btn100/> .
@prefix tipoDeItinerario:  <http://geo.linkeddata.es/kos/transports/tipoDeItinerario/> .
@prefix categoriaDeCurvaDeNivel:  <http://geo.linkeddata.es/kos/geodesy/categoriaDeCurvaDeNivel/> .
