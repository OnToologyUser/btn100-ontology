                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (ontology:FuenteArtificial ontology:Pozo)
                        ]
              ] ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:allValuesFrom ontology:CorrienteSubterranea ;
                            owl:onProperty ontology:permite
