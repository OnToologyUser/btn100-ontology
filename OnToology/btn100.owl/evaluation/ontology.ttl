      owl:equivalentClass
              [ a       owl:Class ;
                owl:unionOf ([ a       owl:Class ;
                            owl:intersectionOf ([ a       owl:Restriction ;
                                        owl:allValuesFrom ontology:AguasMarinas ;
                                        owl:onProperty ontology:desembocaEn
                                      ] [ a       owl:Restriction ;
                                        owl:cardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onProperty ontology:desembocaEn
                                      ])
                          ] [ a       owl:Restriction ;
                            owl:allValuesFrom
                                    [ a       owl:Class ;
                                      owl:unionOf (ontology:Lago ontology:Rio)
                                    ] ;
                            owl:onProperty ontology:desagua_en
                          ])
              ] ;
      owl:equivalentClass
              [ a       owl:Restriction ;
                owl:onProperty ontology:consta_de ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (ontology:AguasDeTransicion ontology:Corriente ontology:Embalse ontology:HidronimoPuntual ontology:Morfologia)
                        ]
              ] .

