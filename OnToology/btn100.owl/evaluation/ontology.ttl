      rdfs:label "Estación invernal"@es ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf (ontology:Alojamiento ontology:LugarDeOcio)
              ] .

ontology:PuertoDeMontanna
