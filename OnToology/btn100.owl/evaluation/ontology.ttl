@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix transportes:  <http://geo.linkeddata.es/kos/transports/> .
@prefix geo:     <http://geo.linkeddata.es/> .
@prefix wgs84_pos:  <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix competenciaDeVia:  <http://geo.linkeddata.es/kos/transports/competenciaDeVia/> .
@prefix acceso:  <http://geo.linkeddata.es/kos/transports/acceso/> .
@prefix rutaTenT:  <http://geo.linkeddata.es/kos/transports/rutaTenT/> .
@prefix categoriaVial:  <http://geo.linkeddata.es/kos/transports/categoriaVial/> .
@prefix ontology:  <http://geo.linkeddata.es/ontology/> .
@prefix electrificacion:  <http://geo.linkeddata.es/kos/transports/electrificacion/> .
@prefix geosparql:  <http://www.opengis.net/ont/geosparql#> .
@prefix categoriaDeRio:  <http://geo.linkeddata.es/kos/hydrography/categoriaDeRio/> .
@prefix competenciaAeroportuaria:  <http://geo.linkeddata.es/kos/transports/competenciaAeroportuaria/> .
@prefix competenciaPortuaria:  <http://geo.linkeddata.es/kos/transports/competenciaPortuaria/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix tipoDeCalzada:  <http://geo.linkeddata.es/kos/transports/tipoDeCalzada/> .
@prefix tipoDeTrafico:  <http://geo.linkeddata.es/kos/transports/tipoDeTrafico/> .
@prefix tipoDeComponente:  <http://geo.linkeddata.es/kos/supply-net/tipoDeComponente/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .

ontology:Canalillo
      a       owl:Class ;
      rdfs:comment "Canal pequenno de conduccion de aguas"^^xsd:string ;
      rdfs:label "Canalillo"@es ;
      rdfs:subClassOf ontology:CanalAguasContinentales ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:conduce ;
                owl:someValuesFrom ontology:AguasSuperficiales
              ] ;
      dc:source "Moliner, Maria. Diccionario de uso del espannol. Madrid: Editorial Gredos. 1984. p. 1590. ISBN: 84-249-1344-2"^^xsd:string .

ontology:VerticeGeodesicoRegente
      a       owl:Class ;
      rdfs:comment "Perteneciente a la Red de Regente"@es ;
      rdfs:label "VerticeGeodesicoRegente"@es ;
      rdfs:subClassOf ontology:VerticeGeodesico .

estadoDeUso2:enConstruccion
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "indica que está en construcción o en obras"@es ;
      skos:inScheme transportes:estadoDeUso ;
      skos:prefLabel "en construcción"@es .

ontology:Regato
      a       owl:Class ;
      rdfs:comment "Arroyo muy pequeño"@es ;
      rdfs:label "Regato"@es ;
      rdfs:subClassOf ontology:Arroyo ;
      dc:source "Moliner, Maria. (1984) Diccionario de uso del espannol. Madrid: Editorial Gredos. pp. 1590. ISBN: 84-249-1344-2"^^xsd:string .

ontology:CapaDeHielo
      a       owl:Class ;
      rdfs:comment "Hielo en la superficie libre de una masa de agua, por ejemplo un lago o un rio"^^xsd:string ;
      rdfs:label "Campo de hielo"@es ;
      rdfs:subClassOf ontology:Nevero ;
      ontology:provenance "Glosario Hidrologico Internacional - UNESCO"^^xsd:string ;
      dc:source "Glosario Hidrologico Internacional - UNESCO"^^xsd:string .

ontology:categoriaDeRio
      a       owl:ObjectProperty ;
      rdfs:comment "Indica el orden de interés de un río"@es ;
      rdfs:label "categoría de río"@es ;
      rdfs:range
              [ a       owl:Class ;
                owl:intersectionOf (skos:Concept [ a       owl:Restriction ;
                            owl:hasValue hydro:categoriaDeRio ;
                            owl:onProperty skos:inScheme
                          ])
              ] .

ontology:fondo
      a       owl:DatatypeProperty ;
      rdfs:domain ontology:AguasSubterraneas ;
      rdfs:range xsd:string .

ontology:Glorieta
      a       owl:Class ;
      rdfs:comment "Plaza donde desembocan por lo común varias calles o alamedas."@es ;
      rdfs:label "Glorieta"@es , "Roundabout"@en ;
      rdfs:subClassOf geosparql:Feature .

ontology:Ria
      a       owl:Class ;
      rdfs:comment "Parte del rio proxima a su desembocadura donde las aguas dulces se mezclan con las marinas durante la marea"^^xsd:string ;
      rdfs:label "Ría"@es , "Ria"@en ;
      rdfs:subClassOf ontology:AguasDeTransicion ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:es_comunicada_por ;
                owl:someValuesFrom ontology:Gola
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9."^^xsd:string .

ontology:LineaElectrica
      a       owl:Class ;
      rdfs:comment "Conjunto de hilos (o cables) y otras instalaciones con fines conducir energía eléctrica. Una Línea eléctrica está constituida tanto por el elemento conductor (usualmente cables de acero, cobre o aluminio) como por sus elementos de soporte (las torres de alta tensión)."@es ;
      rdfs:label "Linea eléctrica"@es ;
      rdfs:subClassOf geosparql:Feature .

ontology:AltaMar
      a       owl:Class ;
      rdfs:comment "Todas aquellas partes del mar que no forman parte del mar territorial o de las aguas interiores, que aparecen situadas fuera de las doscientas millas."^^xsd:string ;
      rdfs:label "Alta mar"@es , "Aguas Internacionales"@es ;
      rdfs:subClassOf ontology:AguasMarinas ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:parte_de ;
                owl:someValuesFrom ontology:Mar
              ] ;
      dc:source "Diez de Velasco, Manuel (1999) Instituciones de derecho internacional publico. Duodecima edicion. Madrid: Tecnos. 924 p. ISBN: 84-309-3446-4"^^xsd:string .

ontology:sometido_a_descarga_por
      a       owl:ObjectProperty ;
      rdfs:domain ontology:Humedal ;
      rdfs:range ontology:AguasSubterraneas ;
      rdfs:subPropertyOf ontology:sometido .

ontology:Helipuerto
      a       owl:Class ;
      rdfs:comment "Pista destinada al aterrizaje y despegue de helicópteros."@es ;
      rdfs:label "Helipuerto"@es ;
      rdfs:subClassOf geosparql:Feature ;
      terms:source "Diccionario de la Real Academia Española"@es .

ontology:ZonaFerroviaria
      a       owl:Class ;
      rdfs:comment "Instalaciones pertenecientes o destinadas para la explotación de la infraestructura del ferrocarril."@es ;
      rdfs:label "Zona ferroviaria"@es ;
      rdfs:subClassOf ontology:ZonaDeUsoCaracteristico .

ontology:Telesqui
      a       owl:Class ;
      rdfs:comment " Aparato que permite a los esquiadores subir hasta las pistas sobre sus esquís mediante un sistema de arrastre."@es ;
      rdfs:label "Telesquí"@es ;
      rdfs:subClassOf ontology:TransporteSuspendido ;
      terms:source "Diccionario de la Real Academia Española."@es .

ontology:AguasCorrientes
      a       owl:Class ;
      rdfs:comment "Aguas continentales situadas en la superficie del suelo que tienen o pueden tener un movimiento notable y continuo a traves de un cauce de origen natural o artificial."^^xsd:string ;
      rdfs:label "Aguas corrientes"@es , "Curso fluvial"@es , "Flowing waters"@en , "Curso de agua"@es , "Cauce"@es , "Flujo fluvial"@es , "Corriente de agua"@es ;
      rdfs:subClassOf ontology:AguasSuperficiales ;
      ontology:provenance "Corriente de agua - Glosario Hidrologico Internacional - UNESCO"^^xsd:string , "Curso de agua - Data Specification. INSPIRE"^^xsd:string , "Flujo fluvial - Glosario Hidrologico Internacional. UNESCO"^^xsd:string ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:unionOf (ontology:AguasCorrientesArtificiales ontology:AguasCorrientesNaturales)
              ] .

population-buildings:estadoDeUso
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """indica el estado de uso. Puede tomar los valores:
* http://geo.linkeddata.es/kos/population-buildings/estadoDeUso/enConstruccion
* http://geo.linkeddata.es/kos/population-buildings/estadoDeUso/enUso
* http://geo.linkeddata.es/kos/population-buildings/estadoDeUso/abandonado"""@es ;
      skos:prefLabel "estado de uso"@es .

ontology:ZonaContigua
      a       owl:Class ;
      rdfs:comment "Zona de alta mar contigua al mar territorial que no puede extenderse mas alla de veinticuatro millas marinas contadas desde las lineas de base a, a partir de las cuales se mide la anchura del mar territorial."^^xsd:string ;
      rdfs:label "Zona contigua"@es ;
      rdfs:subClassOf ontology:AguasMarinas ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:parte_de ;
                owl:someValuesFrom ontology:Mar
              ] ;
      dc:source "Diez de Velasco, Manuel (1999) Instituciones de derecho internacional publico. Duodecima edicion. Madrid: Tecnos. 924 p. ISBN: 84-309-3446-4"^^xsd:string .

btn100:relieve
      a       owl:NamedIndividual , skos:Concept ;
      skos:inScheme kos:btn100 ;
      skos:notation "02" ;
      skos:prefLabel "relieve"@es .

ontology:rutaTenT
      a       owl:ObjectProperty ;
      rdfs:comment "indica si el tramo de vía pertenece a la Red Transeuropea de Transporte (TEN-T)"@es ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (ontology:Aerodromo ontology:Aeropuerto ontology:Autopista ontology:Autovia ontology:Carretera ontology:Ferrocarril ontology:Helipuerto)
              ] ;
      rdfs:label "ruta TEN-T"@es ;
      rdfs:range
              [ a       owl:Class ;
                owl:intersectionOf (skos:Concept [ a       owl:Restriction ;
                            owl:hasValue transportes:rutaTenT ;
                            owl:onProperty skos:inScheme
                          ])
              ] .

ontology:Autovia
      a       owl:Class ;
      rdfs:comment "Carretera con calzadas separadas para los dos sentidos de la circulación, cuyas entradas y salidas no se someten a las exigencias de seguridad de las autopistas."@es ;
      rdfs:label "Autovia"@es ;
      rdfs:subClassOf geosparql:Feature ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Class ;
                            owl:unionOf ([ a       owl:Restriction ;
                                        owl:hasValue tipoDeCalzada:doble ;
                                        owl:onProperty ontology:calzada
                                      ] [ a       owl:Restriction ;
                                        owl:hasValue tipoDeCalzada:sentidoUnico ;
                                        owl:onProperty ontology:calzada
                                      ])
                          ] [ a       owl:Class ;
                            owl:unionOf ([ a       owl:Restriction ;
                                        owl:hasValue estadoDeUso2:enConstruccion ;
                                        owl:onProperty ontology:estadoDeUso
                                      ] [ a       owl:Restriction ;
                                        owl:hasValue estadoDeUso2:enUso ;
                                        owl:onProperty ontology:estadoDeUso
                                      ])
                          ])
              ] ;
      terms:source "Anexo A: Catálogo de Objetos Geográficos de BTN100."@es .

geopolitical:territory
      a       owl:Class .

capital:noCapital
      a       owl:NamedIndividual , skos:Concept ;
      skos:inScheme capital:tipoDeCapital ;
      skos:prefLabel "no es capital"@es .

ontology:Canyo
      a       owl:Class ;
      rdfs:comment "Corriente de agua salada. Puede ser un cauce maritimo angosto que se interna en tierra, una comunicacion entre una albufera y el mar, o un cauce despejado entre una marisma"^^xsd:string ;
      rdfs:label "Caño"@es ;
      rdfs:subClassOf ontology:CanalAguasMarinas , ontology:ZonaHumeda ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:estaEn ;
                owl:someValuesFrom ontology:Marisma
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:comunica_con ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:intersectionOf (ontology:Albufera ontology:Mar ontology:Marisma)
                        ]
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. (1987) Vocabulario de terminos geograficos. Ed. Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. Madrid. ISBN: 84-505-4929-9."^^xsd:string .

transportes:situacionDeVia
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """Disposición del tramo de vía sobre la superficie.

* http://geo.linkeddata.es/kos/transports/situacionDeVia/elevado
* http://geo.linkeddata.es/kos/transports/situacionDeVia/subterraneo
* http://geo.linkeddata.es/kos/transports/situacionDeVia/superficial"""@es ;
      skos:prefLabel "situación"@es .

ontology:eutrofizacion
      a       owl:DatatypeProperty ;
      rdfs:domain ontology:Embalse ;
      rdfs:range xsd:float .

ontology:Saladar
      a       owl:Class ;
      rdfs:comment "Lagunazo en que se cuaja la sal en las marismas"@es ;
      rdfs:label "Saladar"@es ;
      rdfs:subClassOf ontology:Salina ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:esta_proxima_a ;
                owl:someValuesFrom ontology:Marisma
              ] ;
      ontology:provenance "Getty Thesaurus of Geographic Names"@en ;
      dc:source "Diccionario de la Real Academia Española"^^xsd:string .

ontology:Venaje
      a       owl:Class ;
      rdfs:comment "Conjunto de venas de agua y manantiales que dan origen a un río"@es ;
      rdfs:label "Venaje"@es ;
      rdfs:subClassOf ontology:SurgenciaNatural ;
      dc:source "Diccionario de la Real Academia Española"@es .

ontology:Aeroclub
      a       owl:Class ;
      rdfs:comment "Sociedad recreativa interesada por el deporte aéreo."@es ;
      rdfs:label "Aeroclub"@en , "Aeroclub"@es ;
      rdfs:subClassOf geosparql:Feature .

[]    a       owl:AllDifferent ;
      owl:distinctMembers (acceso:libre acceso:peaje) .

categoriaDeCurvaDeNivel:curvaDeNivelInterpolada
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment """Curva con altitud resultado de un algoritmo de cálculo y no de los
datos del terreno."""@es ;
      rdfs:label "curva de nivel interpolada"@es ;
      skos:inScheme geodesy:categoriaDeCurvaDeNivel .

ontology:estado
      a       owl:DatatypeProperty ;
      rdfs:domain ontology:Aguas ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:oneOf
                        [ a       rdf:List ;
                          rdf:first "liquido"^^xsd:string ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "solido"^^xsd:string ;
                                    rdf:rest ()
                                  ]
                        ]
              ] .

ontology:EnclaveFerroviario
      a       owl:Class ;
      rdfs:comment "instalaciones necesarias para el  funcionamiento del ferrocarril en todas sus faceta."@es ;
      rdfs:label "Enclave Ferroviario"@es ;
      rdfs:subClassOf geosparql:Feature ;
      terms:source "Administrador de Infraestructuras Ferroviarias (ADIF)"@es .

ontology:VerticeGeodesicoRegcan95
      a       owl:Class ;
      rdfs:comment "Perteneciente a REGCAN95"@es ;
      rdfs:label "VerticeGeodesicoRegcan95"@es ;
      rdfs:subClassOf ontology:VerticeGeodesico .

competenciaAeroportuaria:militar
      a       owl:NamedIndividual , skos:Concept ;
      rdfs:comment "competencia aeroportuaria de explotación militar"@es ;
      skos:inScheme transportes:competenciaAeroportuaria ;
      skos:prefLabel "militar"@es .

ontology:almacena
      a       owl:ObjectProperty ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (ontology:Aljibe ontology:Embalse)
              ] ;
      rdfs:range
              [ a       owl:Class ;
                owl:unionOf (ontology:Arroyo ontology:Manantial ontology:Rio)
              ] ;
      owl:inverseOf ontology:es_almacenado_en .

ontology:AreaDeServicio
      a       owl:Class ;
      rdfs:comment "En una autopista o autovía, lugar habilitado para el estacionamiento de los vehículos, en el que suele haber una gasolinera, un restaurante y otros servicios. "@es ;
      rdfs:label "Area de servicio"@es ;
      rdfs:subClassOf ontology:ZonaDeUsoCaracteristico .

ontology:transmite
      a       owl:ObjectProperty ;
      rdfs:domain ontology:Acuitardo ;
      rdfs:range ontology:AguasSubterraneas ;
      rdfs:subPropertyOf ontology:permite .

ontology:EstanqueSalado
      a       owl:Class ;
      rdfs:label "Estanque salado"@es ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf (ontology:AguasContinentales ontology:AguasSaladas)
              ] .

ontology:discurre_sobre
      a       owl:ObjectProperty ;
      rdfs:comment ""^^xsd:string ;
      rdfs:domain ontology:AguasCorrientesNaturales ;
      rdfs:range ontology:AguasCorrientesNaturales .

ontology:Gasolinera
      a       owl:Class ;
      rdfs:comment "Punto de venta de combustible para vehículos de motor."@es ;
      rdfs:label "Gasolinera"@es ;
      rdfs:subClassOf ontology:LugarDeInteres .

ontology:Albergue
      a       owl:Class ;
      rdfs:comment "Lugar que renta alojamiento, particularmente alentando las actividades al aire libre y el intercambio cultural entre jóvenes de distintos países."@es ;
      rdfs:label "Albergue"@es ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf (ontology:Alojamiento ontology:LugarDeOcio)
              ] .

ontology:Acequia
      a       owl:Class ;
      rdfs:comment "Zanja o canal por donde son conducidas las aguas para el riego o para otros usos."^^xsd:string ;
      rdfs:label "Irrigation ditch"@en , "Acequia"@es ;
      rdfs:subClassOf ontology:AguasCorrientesArtificiales ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom ontology:AguasCorrientesNaturales ;
                owl:onProperty ontology:conduce
              ] ;
      dc:source "Vazquez Maure F.; Martin Lopez J. Vocabulario de terminos geograficos. Madrid: Ministerio de Obras Publicas y Urbanismo. Instituto Geografico Nacional. 1987. 89 p. ISBN: 84-505-4929-9."^^xsd:string .

hydro:categoriaDeIsla
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """Puede tomar los valores:
* http://geo.linkeddata.es/kos/hydrography/categoriaDeIsla/principal"""@es ;
      skos:prefLabel "categoría de isla"@es .

ontology:FerrocarrilDeAltaVelocidad
      a       owl:Class ;
      rdfs:comment "Anexo A: Catálogo de Objetos Geográficos de BTN100."@es , "Tramo de vía por la que circula el Tren de alta velocidad de España (AVE)."@es ;
      rdfs:label "Tramo de vía de alta velocidad"@es ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf (ontology:Ferrocarril [ a       owl:Class ;
                            owl:unionOf ([ a       owl:Restriction ;
                                        owl:hasValue estadoDeUso2:enConstruccion ;
                                        owl:onProperty ontology:estadoDeUso
                                      ] [ a       owl:Restriction ;
                                        owl:hasValue estadoDeUso2:enUso ;
                                        owl:onProperty ontology:estadoDeUso
                                      ])
                          ])
              ] .

ontology:descarga_en
      a       owl:ObjectProperty ;
      rdfs:domain ontology:AguasSubterraneas ;
      rdfs:range ontology:ZonaHumeda ;
      owl:inverseOf ontology:sufre_descarga_de .

transportes:tipoDeTrafico
      a       owl:NamedIndividual , skos:ConceptScheme ;
      rdfs:comment """kos que indica el tipo de tráfico que lleva un ferrocarril. Pueden ser
* http://geo.linkeddata.es/kos/transporters/tipoDeTrafico/viajeros
* http://geo.linkeddata.es/kos/transporters/tipoDeTrafico/mercancias
* http://geo.linkeddata.es/kos/transporters/tipoDeTrafico/mixto"""@es ;
      skos:prefLabel "tipo de tráfico"@es .

ontology:consta_de
      a       owl:TransitiveProperty , owl:ObjectProperty ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (ontology:AguasMarinas ontology:Arroyo ontology:Embalse ontology:Lago ontology:Laguna ontology:Rio)
              ] ;
      rdfs:range
              [ a       owl:Class ;
                owl:unionOf (ontology:AguasDeTransicion ontology:Corriente ontology:Embalse ontology:HidronimoPuntual ontology:Morfologia)
              ] .

ontology:competenciaDeVia
      a       owl:ObjectProperty ;
      rdfs:comment "competencia de la explotación de una vía"@es ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (ontology:Autopista ontology:Autovia ontology:CarreteraAutonomica)
              ] ;
      rdfs:label "competencia de vía"@es ;
      rdfs:range
              [ a       owl:Class ;
                owl:intersectionOf (skos:Concept [ a       owl:Restriction ;
                            owl:hasValue transportes:competenciaDeVia ;
                            owl:onProperty skos:inScheme
                          ])
              ] .

ontology:AguasSuperficiales
      a       owl:Class ;
      rdfs:comment "Inland waters, except groundwater; transitional waters and coastal waters, except in respect of chemical status for which it shall also include territorial waters."^^xsd:string , "Aguas que incluyen las aguas continentales, excepto las aguas subterraneas; las aguas de transicion y las aguas costeras, y, en lo que se refiere al estado quimico, tambien las aguas territoriales"^^xsd:string ;
      rdfs:label "Masa de agua superficial"@es , "Aguas Superficiales"@es , "Surface water"@en ;
      rdfs:subClassOf ontology:AguasContinentales ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:es_distribuida ;
                owl:someValuesFrom ontology:Distribucion
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:es_transvasada ;
                owl:someValuesFrom ontology:CanalAguasContinentales
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ontology:es_distribuida ;
                owl:someValuesFrom ontology:CanalAguasContinentales
              ] ;
      ontology:provenance "Directiva Marco del Agua. Union Europea"^^xsd:string ;
      dc:source "Directiva Marco del Agua. Union Europea"^^xsd:string , "European Parliament and the Council of the European Union. DIRECTIVE 2000/60/EC OF THE EUROPEAN PARLIAMENT AND OF THE COUNCIL for establishing a framework for Community action in the field of water policy. Brussels, 23 October 2000. p 72."^^xsd:string ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:unionOf (ontology:AguasCorrientes ontology:AguasDeTransicion ontology:AguasQuietas ontology:Surgencia)
              ] .

regimenDeLaguna:permanente
