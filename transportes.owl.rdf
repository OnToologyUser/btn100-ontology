    <!-- http://geo.linkeddata.es/ontology/transportes.owl#estado -->

    <owl:DatatypeProperty rdf:about="&ontology;transportes.owl#estado">
        <rdfs:label xml:lang="es">estado</rdfs:label>
        <terms:source xml:lang="es">Anexo A: Catálogo de Objetos Geográficos de BTN100.
Tabla ESTADO</terms:source>
        <rdfs:comment xml:lang="es">Determina el estado de uso.</rdfs:comment>
        <rdfs:domain rdf:resource="&ontology;Autopista"/>
        <rdfs:domain rdf:resource="&ontology;Carretera"/>
        <rdfs:domain rdf:resource="&ontology;transportes.owl#Autovia"/>
        <rdfs:domain rdf:resource="&ontology;transportes.owl#Enlace"/>
        <rdfs:domain rdf:resource="&ontology;transportes.owl#Ferrocarril"/>
        <rdfs:domain rdf:resource="&ontology;transportes.owl#FerrocarrilConvencional"/>
        <rdfs:domain rdf:resource="&ontology;transportes.owl#FerrocarrilDeAltaVelocidad"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">abandonado</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">en construcción</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">en uso</rdf:first>
                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


