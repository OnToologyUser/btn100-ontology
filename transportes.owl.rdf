    <!-- http://geo.linkeddata.es/ontology/acceso -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/acceso">
        <rdfs:label xml:lang="es">acceso</rdfs:label>
        <rdfs:comment xml:lang="es">Indica se la vía es libre o de peaje</rdfs:comment>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/Autopista"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">de peaje</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">libre</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


