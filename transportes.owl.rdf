    <!-- http://geo.linkeddata.es/ontology/transportes.owl#tipoDeCalzada -->

    <owl:DatatypeProperty rdf:about="&ontology;transportes.owl#tipoDeCalzada">
        <rdfs:label xml:lang="es">tipoDeCalzada</rdfs:label>
        <terms:source xml:lang="es">Anexo A: Catálogo de Objetos Geográficos de BTN100.
Tabla CALZADA</terms:source>
        <rdfs:comment xml:lang="es">Indica el número de calzadas y sentido de circulación del tramo de vía</rdfs:comment>
        <rdfs:domain rdf:resource="&ontology;Autopista"/>
        <rdfs:domain rdf:resource="&ontology;Carretera"/>
        <rdfs:domain rdf:resource="&ontology;transportes.owl#Autovia"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">convencional (una calzada con doble sentido de cirulación)</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">doble (dos calzadas separadas y paralelas, con sentidos distintos)</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">sentido único (calzada de sentido único no contigua a la del contrario)</rdf:first>
                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://geo.linkeddata.es/ontology/transportes.owl#tipoDeCompetenciaVial -->

    <owl:DatatypeProperty rdf:about="&ontology;transportes.owl#tipoDeCompetenciaVial">
        <rdfs:label xml:lang="es">tipoDeCompetenciaVial</rdfs:label>
        <terms:source xml:lang="es">Anexo A: Catálogo de Objetos Geográficos de BTN100.
Tabla COMPE</terms:source>
        <rdfs:comment xml:lang="es">Determina de quien es competencia la via.
ESTATAL : La competencia es del Estado
AUTONÓMICA : La competencia es de la administración de las CCAA
PROVINCIAL : La competencia es de la Diputación Provincial
LOCAL :La competencia es del Municipio
</rdfs:comment>
        <rdfs:domain rdf:resource="&ontology;Autopista"/>
        <rdfs:domain rdf:resource="&ontology;Carretera"/>
        <rdfs:domain rdf:resource="&ontology;transportes.owl#Autovia"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">autonómica</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">estatal</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">local</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first rdf:datatype="&xsd;string">provincial</rdf:first>
                                                <rdf:rest rdf:resource="&rdf;nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


