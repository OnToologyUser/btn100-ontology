    <!-- http://geo.linkeddata.es/ontology/itinerarioEuropeo -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/itinerarioEuropeo">
        <rdfs:label xml:lang="es">itinerario europeo</rdfs:label>
        <terms:source xml:lang="es">Anexo A: Catálogo de Objetos Geográficos de BTN100.
Tabla ITINERARIO EUROPEO</terms:source>
        <rdfs:comment xml:lang="es">Indica si el tramo de vía perteneciente a la Red de Carreteras Europeas</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Autopista"/>
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Autovia"/>
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Carretera"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">itinerario europeo</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">itinerario no europeo</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


