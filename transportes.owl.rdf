    <!-- http://geo.linkeddata.es/ontology/transportes.owl#trafico -->

    <owl:DatatypeProperty rdf:about="&ontology;transportes.owl#trafico">
        <rdfs:label xml:lang="es">tráfico</rdfs:label>
        <rdfs:comment xml:lang="es">Tipo de tráfico de la estación</rdfs:comment>
        <rdfs:domain rdf:resource="&ontology;transportes.owl#EnclaveFerroviario"/>
        <rdfs:domain rdf:resource="&ontology;transportes.owl#FerrocarrilConvencional"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">desconocido</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">mercancías</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">viajeros</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
