    <!-- http://geo.linkeddata.es/ontology/estadoDeUso -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/estadoDeUso">
        <rdfs:label xml:lang="es">estado</rdfs:label>
        <terms:source xml:lang="es">Anexo A: Catálogo de Objetos Geográficos de BTN100.
Tabla ESTADO</terms:source>
        <rdfs:comment xml:lang="es">Determina el estado de uso.</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Autopista"/>
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Autovia"/>
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Carretera"/>
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Enlace"/>
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Ferrocarril"/>
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Puerto"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">abandonado</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">en construcción</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">en uso</rdf:first>
                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


