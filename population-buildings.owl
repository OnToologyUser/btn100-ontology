    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#bicCode -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#bicCode">
        <rdfs:label xml:lang="es">bicCode</rdfs:label>
        <rdfs:comment xml:lang="es">Se incluye el código BIC que define el bien. Caso de no ser un Lugar de interés BIC, se rellenará con el texto: NO PROCEDE.</rdfs:comment>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeInteres"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#estado -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#estado">
        <rdfs:label xml:lang="es">estado</rdfs:label>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#ExplotacionMinera"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">abandonado</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">en construcción</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">en uso</rdf:first>
                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#ineCode -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#ineCode">
        <rdfs:label xml:lang="en">ineCode</rdfs:label>
        <rdfs:label xml:lang="es">codigo ine</rdfs:label>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#DiseminadoPuntual"/>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#DiseminadoSuperficial"/>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#NucleoPoblacionalPuntual"/>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#NucleoPoblacionalSuperficial"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:length rdf:datatype="&xsd;integer">11</xsd:length>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#poblacion -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#poblacion">
        <rdfs:label xml:lang="es">poblacion</rdfs:label>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#NucleoPoblacionalPuntual"/>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#NucleoPoblacionalSuperficial"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#tipoDeCapital -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#tipoDeCapital">
        <rdfs:label xml:lang="es">tipoDeCapital</rdfs:label>
        <rdfs:comment xml:lang="es">Especifica de qué administración oficial es capital</rdfs:comment>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#DiseminadoPuntual"/>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#DiseminadoSuperficial"/>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#NucleoPoblacionalPuntual"/>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#NucleoPoblacionalSuperficial"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">capital de comunidad autónoma</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">capital de comunidad autónoma pero no de provincia</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">capital de nación</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first rdf:datatype="&xsd;string">capital de provincia</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                        <rdf:first rdf:datatype="&xsd;string">capital de un municipio</rdf:first>
                                                        <rdf:rest>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="&rdf;List"/>
                                                                <rdf:first rdf:datatype="&xsd;string">no es capital</rdf:first>
                                                                <rdf:rest rdf:resource="&rdf;nil"/>
                                                            </rdf:Description>
                                                        </rdf:rest>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


