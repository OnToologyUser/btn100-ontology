    <!-- http://geo.linkeddata.es/ontology/bicCode -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/bicCode">
        <rdfs:label xml:lang="es">código big</rdfs:label>
        <rdfs:comment xml:lang="en">big code</rdfs:comment>
        <rdfs:comment xml:lang="es">Se incluye el código BIC que define el bien. Caso de no ser un Lugar de interés BIC, se rellenará con el texto: NO PROCEDE.</rdfs:comment>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/LugarDeInteres"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://geo.linkeddata.es/ontology/esCapitalDe -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/esCapitalDe">
        <rdfs:label xml:lang="es">es capital de</rdfs:label>
        <rdfs:comment xml:lang="es">Especifica de qué administración oficial es capital</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Diseminado"/>
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/NucleoPoblacional"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">capital de comunidad autónoma</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">capital de comunidad autónoma pero no de provincia</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">capital de nación</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first rdf:datatype="&xsd;string">capital de provincia</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                        <rdf:first rdf:datatype="&xsd;string">capital de un municipio</rdf:first>
                                                        <rdf:rest>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="&rdf;List"/>
                                                                <rdf:first rdf:datatype="&xsd;string">no es capital</rdf:first>
                                                                <rdf:rest rdf:resource="&rdf;nil"/>
                                                            </rdf:Description>
                                                        </rdf:rest>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://geo.linkeddata.es/ontology/estadoDeUso -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/estadoDeUso">
        <rdfs:label xml:lang="es">estado</rdfs:label>
        <terms:source xml:lang="es">Anexo A: Catálogo de Objetos Geográficos de BTN100.
Tabla ESTADO</terms:source>
        <rdfs:comment xml:lang="es">Estado de uso.</rdfs:comment>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/ExplotacionMinera"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">abandonado</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">en construcción</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">en uso</rdf:first>
                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://geo.linkeddata.es/ontology/ineCode -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/ineCode">
        <rdfs:label xml:lang="en">ine code</rdfs:label>
        <rdfs:label xml:lang="es">codigo ine</rdfs:label>
        <rdfs:comment xml:lang="es">Codificación asignada por el Nomenclátor INE.</rdfs:comment>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:length rdf:datatype="&xsd;integer">11</xsd:length>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/Diseminado"/>
                    <rdf:Description rdf:about="http://geo.linkeddata.es/ontology/NucleoPoblacional"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://geo.linkeddata.es/ontology/poblacion -->

    <owl:DatatypeProperty rdf:about="http://geo.linkeddata.es/ontology/poblacion">
        <rdfs:label xml:lang="es">poblacion</rdfs:label>
        <rdfs:comment xml:lang="es">Número total de habitantes del núcleo según el nomenclátor del INE para el año que aparezca en los metadatos.</rdfs:comment>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/NucleoPoblacional"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


