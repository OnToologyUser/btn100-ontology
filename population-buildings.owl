    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Albergue -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Albergue">
        <rdfs:label xml:lang="es">Albergue</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#Alojamiento"/>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeOcio"/>
        <rdfs:comment xml:lang="es">Lugar que renta alojamiento, particularmente alentando las actividades al aire libre y el intercambio cultural entre jóvenes de distintos países.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Alojamiento -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Alojamiento">
        <rdfs:label xml:lang="es">Alojamiento</rdfs:label>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#AreaDeServicio -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#AreaDeServicio">
        <rdfs:label xml:lang="es">AreaDeServicio</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#ZonaDeUsoCaracteristico"/>
        <rdfs:comment xml:lang="es">En una autopista o autovía, lugar habilitado para el estacionamiento de los vehículos, en el que suele haber una gasolinera, un restaurante y otros servicios. </rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Balneario -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Balneario">
        <rdfs:label xml:lang="es">Balneario</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#Alojamiento"/>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeOcio"/>
        <rdfs:comment xml:lang="es">Lugar dedicado al reposo y la curación a través de la utilización de las aguas, sobre todo termales o minerales, con un edificio para el hospedaje. </rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Camping -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Camping">
        <rdfs:label xml:lang="es">Camping</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#Alojamiento"/>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeOcio"/>
        <rdfs:comment xml:lang="es">Lugar al aire libre, especialmente dispuesto para albergar viajeros, turistas, personas en vacaciones, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Cantera -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Cantera">
        <rdfs:label xml:lang="es">Cantera</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#ExplotacionMinera"/>
        <rdfs:comment xml:lang="es"> Sitio de donde se saca piedra, greda u otra sustancia análoga para obras varias</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Castillo -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Castillo">
        <rdfs:label xml:lang="es">Castillo</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeInteres"/>
        <rdfs:comment xml:lang="es">Lugar fuerte, cercado de murallas, baluartes, fosos y otras fortificaciones</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Cementerio -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Cementerio">
        <rdfs:label xml:lang="es">Cementerio</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeInteres"/>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#ZonaDeUsoCaracteristico"/>
        <rdfs:comment xml:lang="es">Lugar, generalmente cercado, destinado a enterrar los restos mortales o cadáveres de los difuntos. </rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#ConjuntoHistoricoArtistico -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#ConjuntoHistoricoArtistico">
        <rdfs:label xml:lang="es">ConjuntoHistoricoArtistico</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeInteres"/>
        <rdfs:comment xml:lang="es">Es la agrupación de inmuebles que forman una unidad de asentamiento, continua o dispersa, condicionada por una estructura física representativa de la evolución de una comunidad humana por ser testimonio de su cultura o constituir un valor de uso y disfrute para la colectividad.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Cueva -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Cueva">
        <rdfs:label xml:lang="es">Cueva</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeInteres"/>
        <rdfs:comment xml:lang="es">Cavidad natural del terreno causada por algún tipo de erosión de corrientes de agua, hielo o lava, o una combinación de varios de estos factores.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Diseminado -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Diseminado">
        <rdfs:label xml:lang="es">Diseminado</rdfs:label>
        <owl:disjointWith rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#NucleoPoblacional"/>
        <terms:source xml:lang="es">Instituto nacional de estadística.</terms:source>
        <rdfs:comment xml:lang="es">Las edificaciones o viviendas de una entidad singular de población que no pueden ser incluidas en el concepto de núcleo de población se consideran pertenecientes al diseminado, entendiendo así por diseminado aquellas casas aisladas que no están recogidas como núcleo de población por el INE.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#DiseminadoPuntual -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#DiseminadoPuntual">
        <rdfs:label xml:lang="es">DiseminadoPuntual</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#Diseminado"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#tipoDeCapital"/>
                        <owl:hasValue rdf:datatype="&xsd;string">capital de un municipio</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#tipoDeCapital"/>
                        <owl:hasValue rdf:datatype="&xsd;string">no es capital</owl:hasValue>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#DiseminadoSuperficial"/>
        <terms:source xml:lang="es">Anexo A: Catálogo de Objetos Geográficos de BTN100.</terms:source>
        <rdfs:comment xml:lang="es">Existen dos tipos de elementos que se engloban en el concepto de Diseminado puntual:

* Edificación aislada: caseríos, cortijos, etc. en los que o bien existe una única edificación, o siendo un pequeño conjunto de edificaciones (dos, tres o cuatro...) están dispuestas de tal forma que entre ellas no existe una distancia superior a 100m sobre la imagen.

* Edificación dispersa: es el caso de los diseminados comúnmente conocidos. Esto es, aquel conjunto considerable de edificaciones (cinco o más) que sin conformar un núcleo de población presentan características propias de estas (habitabilidad, comunicaciones, etc.).En este caso el objeto se representará por su centroide.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#DiseminadoSuperficial -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#DiseminadoSuperficial">
        <rdfs:label xml:lang="es">DiseminadoSuperficial</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#Diseminado"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#tipoDeCapital"/>
                        <owl:hasValue rdf:datatype="&xsd;string">capital de un municipio</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#tipoDeCapital"/>
                        <owl:hasValue rdf:datatype="&xsd;string">no es capital</owl:hasValue>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="es">Agrupación de edificaciones que se extienden por una superficie considerable con estructura similar a un núcleo pero que no aparecen en el INE clasificado como tal. Es el caso de urbanizaciones, zonas residenciales u otras estructuras de población similar.</rdfs:comment>
        <terms:source xml:lang="es">Anexo A: Catálogo de Objetos Geográficos de BTN100.</terms:source>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#EdificioReligioso -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#EdificioReligioso">
        <rdfs:label xml:lang="es">EdificioReligioso</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeInteres"/>
        <rdfs:comment xml:lang="es">Templo destinado al culto religioso</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#EdificioSingular -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#EdificioSingular">
        <rdfs:label xml:lang="es">EdificioSingular</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeInteres"/>
        <rdfs:comment xml:lang="es">Comprende los edificios que deberán ser conservados íntegramente por su carácter singular y monumental, y por razones histórico-artísticas, preservando todas sus características arquitectónicas.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#EstacionInvernal -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#EstacionInvernal">
        <rdfs:label xml:lang="es">EstacionInvernal</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#Alojamiento"/>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeOcio"/>
        <rdfs:comment xml:lang="es">Área preparada para el disfrute de la nieve. Dispone de pistas de esquí balizadas preparadas por máquinas especiales así como pistas de fondo, hoteles para el descanso de los usuarios, remontes para el transporte de los esquiadores a zonas altas y diferentes servicios que pueden ir desde zona para usar trineos hasta pistas de patinaje.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#ExplotacionMinera -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#ExplotacionMinera">
        <rdfs:label xml:lang="es">ExplotacionMinera</rdfs:label>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#estado"/>
                        <owl:hasValue rdf:datatype="&xsd;string">abandonado</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#estado"/>
                        <owl:hasValue rdf:datatype="&xsd;string">en uso</owl:hasValue>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="es">Terreno dedicado a la explotación de minerales o piedra.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Gasolinera -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Gasolinera">
        <rdfs:label xml:lang="es">Gasolinera</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeInteres"/>
        <rdfs:comment xml:lang="es">Punto de venta de combustible para vehículos de motor.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Hospital -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Hospital">
        <rdfs:label xml:lang="es">Hospital</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeInteres"/>
        <rdfs:comment xml:lang="es">Establecimiento sanitario donde se atiende a los enfermos para proporcionar el diagnóstico y tratamiento que necesitan.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#InstalacionDeportiva -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#InstalacionDeportiva">
        <rdfs:label xml:lang="es">InstalacionDeportiva</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#Alojamiento"/>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#LugarDeOcio"/>
        <rdfs:comment xml:lang="es">Recinto o construcción provista de los medios necesarios para el aprendizaje, la práctica y la competición de uno o más deportes. </rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#Invernadero -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#Invernadero">
        <rdfs:label xml:lang="es">Invernadero</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://geo.linkeddata.es/ontology/population-buildings.owl#ZonaDeUsoCaracteristico"/>
        <rdfs:comment xml:lang="es">Lugar preparado artificialmente para cultivar las plantas fuera de su ambiente y clima habituales.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geo.linkeddata.es/ontology/population-buildings.owl#JardinHistorico -->

    <owl:Class rdf:about="http://geo.linkeddata.es/ontology/population-buildings.owl#JardinHistorico">
        <rdfs:label xml:lang="es">JardinHistorico</rdfs:label>
