        <rdfs:comment xml:lang="es">código unlo</rdfs:comment>
        <rdfs:domain rdf:resource="http://geo.linkeddata.es/ontology/Puerto"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:length rdf:datatype="&xsd;integer">3</xsd:length>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


